                                          <xsd:documentation source="CalculationsAndRules">This field is present if  ResidentialFenestrationType equals Window</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="StandardBugScreen">Bug Screen</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>Win:ExteriorShade</dtyp:CBECCresXMLterm>
