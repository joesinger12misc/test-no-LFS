                                             <xsd:documentation source="FieldText">Width (ft)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">This data is present if  ResidentialFenestrationType equals Window</xsd:documentation>
                                             <xsd:appinfo source="CBECCresXML">
                                                <dtyp:CBECCresXMLterm>Win:Width</dtyp:CBECCresXMLterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
