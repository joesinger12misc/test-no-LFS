                                       </xsd:element>
                                       <xsd:element name="LUserDefined_FieldValue" type="com:FieldValue" >
                                       </xsd:element>
                                       <xsd:element name="LUserDefined_FieldValueType" type="com:FieldValueType" >
                                       </xsd:element>
                                    </xsd:sequence>
