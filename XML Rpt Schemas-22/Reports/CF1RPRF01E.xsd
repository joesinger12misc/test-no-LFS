                                             <dtyp:CBECCresXMLterm>Attic:SolReflSpclFtrs</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>Need logic to test value of SolReflSpclFtrs >= 2, then 'Yes' otherwise 'No'</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L09_StatusEAA" type="com:StatusEAA"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This field is required if ProjectScopePerformance in GENERAL INFORMATION == AdditionAlteration And changes involve the Attic</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>Attic:Status</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>I don't see Status field in XSLT or PDF report.  See Calc&amp;Rules condition.</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L10_VerifiedExistingCondition" type="comp:VerifiedExistingCondition"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Verified Existing Condition</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This field is required if ProjectScopePerformance in GENERAL INFORMATION == AdditionAlteration And changes involve the Attic. 
 If L09_StatusEEA == New result = NA; Else  If StatusEEA  == Altered result = Yes; Else If StatusEEA  == Existing result = No.</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>Attic:IsExisting</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L11_ConstructionAssemblyNamePerformanceRes" type="comp:ConstructionAssemblyNamePerformanceRes"  minOccurs="0">
