                                    </xsd:element>
                                    <xsd:element name="M09a_FenestrationCertificationMethod" type="env:FenestrationCertificationMethod" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SHGC Source</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="CECDefaultPerformance">Table 110.6-B</dtyp:displayterm>
