                                          <xsd:restriction base="env:ExternalShadingObjectType">
                                             <xsd:enumeration value="NoExternalShadingDevice"/>
                                             <xsd:enumeration value="StandardBugScreen"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="M11_StatusEAA" type="com:StatusEAA"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This field is required if ProjectScopePerformance in GENERAL INFORMATION equals AdditionAlteration</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>Win:Status</dtyp:CBECCresXMLterm>
