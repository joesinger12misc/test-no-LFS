                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>ExtWall:Name</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>Need logic to get parent surface</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M03b_RelativePosition" type="com:RelativePosition" >
