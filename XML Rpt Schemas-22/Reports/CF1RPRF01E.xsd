         <xsd:element name="Section_N" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">OPAQUE DOORS</xsd:documentation>
               <xsd:appinfo source="CBECCresXML">
                  <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                  <dtyp:termConditions>Requires looping over: 'Proposed']/Proj/Zone/ExtWall/Door, Zone/IntWall[./IsDemising=1]/Door, and Garage/ExtWall/Door</dtyp:termConditions>
               </xsd:appinfo>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableOpaqueDoors" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:appinfo source="CBECCresXML">
                           <dtyp:CBECCresXMLterm>:</dtyp:CBECCresXMLterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="N01_DoorName" type="env:DoorName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>Door:Name</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="N02_PartitionName" type="env:PartitionName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Side of Building</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>Zone:Name</dtyp:CBECCresXMLterm>
                                             <dtyp:termConditions>Parent:Name</dtyp:termConditions>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="N03_DoorArea" type="env:DoorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:appinfo source="CBECCresXML">
                                             <dtyp:CBECCresXMLterm>Door:Area</dtyp:CBECCresXMLterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
