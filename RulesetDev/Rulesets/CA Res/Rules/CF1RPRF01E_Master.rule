RULELIST "CF1RPRF01E_cf1rSection_OverhangVA_Proposed"  1 0 0 1
   "Create & initialize new cf1rSection_OverhangVA object"
      cf1rCF1RPRF01E:Action  = {  CreateChildren( cf1rSection_OverhangVA, 1, "Section_OverhangVA" )  }     ; no init needed
   "Create & initialize new cf1rTableOverhang object"
      cf1rSection_OverhangVA:Action   = {  CreateChildren( cf1rTableOverhangsExisting, 1, "TableOverhangsExisting" )  }     ; no init needed
  ; ----------------------------------------------------------------------
  ; Rules to create individual existing OverhangFins table rows
   "Create cf1rtblRowSlabFloorExisting for all existing SlabFloor types"
      Window:CF1R_OverhangRowExisting   = {  
            if (Proj:RunScope = 1)   ; new construction
            then  UNDEFINED
            else if (IsNew)
            then  UNDEFINED
            else if (IsAltered .AND. IsVerified .AND. ModelFinsOverhang)
            then  CreateComp( cf1rtblRowOverhangExisting, Name + " ExistingOverhangRow" )
            else  UNDEFINED
            endif endif endif }
  ; ----------------------------------------------------------------------
    ; Rules to populate Overhang table row for this Window
      "Set Window:CF1R_OverhangRowExisting:OverhangVA01_FenestrationVerticalName" Window:CF1R_OverhangRowExisting:OverhangVA01_FenestrationVerticalName  = {  Name  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA02_OverhangDepth" Window:CF1R_OverhangRowExisting:OverhangVA02_OverhangDepth  = {  exOverhangDepth  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA03_OverhangDistanceUp" Window:CF1R_OverhangRowExisting:OverhangVA03_OverhangDistanceUp  = {  exOverhangDistUp  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA04_OverhangLeftExtent" Window:CF1R_OverhangRowExisting:OverhangVA04_OverhangLeftExtent  = {  exOverhangExL  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA05_OverhangRightExtent" Window:CF1R_OverhangRowExisting:OverhangVA05_OverhangRightExtent  = {  exOverhangExR  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA06_OverhangFlapHeight" Window:CF1R_OverhangRowExisting:OverhangVA06_OverhangFlapHeight  = {  exOverhangFlap  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA07_FinDepth" Window:CF1R_OverhangRowExisting:OverhangVA07_FinDepth  = {  exLeftFinDepth  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA08_FinTopUp" Window:CF1R_OverhangRowExisting:OverhangVA08_FinTopUp  = {  exLeftFinTopUp  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA09_FinDistanceLeft" Window:CF1R_OverhangRowExisting:OverhangVA09_FinDistanceLeft  = {  exLeftFinDistL  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA10_FinBottomUp" Window:CF1R_OverhangRowExisting:OverhangVA10_FinBottomUp  = {  exLeftFinBotUp  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA11_FinDepth" Window:CF1R_OverhangRowExisting:OverhangVA11_FinDepth  = {  exRightFinDepth  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA12_FinTopUp" Window:CF1R_OverhangRowExisting:OverhangVA12_FinTopUp  = {  exRightFinTopUp  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA13_FinDistanceRight" Window:CF1R_OverhangRowExisting:OverhangVA13_FinDistanceRight  = {  exRightFinDistR  }
      "Set Window:CF1R_OverhangRowExisting:OverhangVA14_FinBottomUp" Window:CF1R_OverhangRowExisting:OverhangVA14_FinBottomUp  = {  exRightFinBotUp  }

  ; ----------------------------------------------------------------------
  ; Rule to set Parent for ALL existing OverhangFins table rows
   "Set cf1rtblRowOverhangExisting parent"   cf1rtblRowOverhangExisting:Parent  = { cf1rTableOverhangsExisting:Name  }
  ; END of existing OverhangFins table rules
  ; ----------------------------------------------------------------------
  ; Rule to Delete empty Existing OverhangFins table
   "DELETE empty cf1rTableOverhangsExisting object"
      cf1rSection_OverhangVA:Action   = {  if (ComponentCount( cf1rtblRowOverhangExisting ) < 1) then  DeleteComp()  else  UNCHANGED  endif }
  ; ----------------------------------------------------------------------
END
