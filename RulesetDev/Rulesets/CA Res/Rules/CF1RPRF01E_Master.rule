RULELIST "CF1RPRF01E_cf1rSection_Aa_Proposed"  1 0 0 1

   "Create & initialize new cf1rTableDistribution object"
      cf1rSection_Aa:Action   = {  CreateChildren( cf1rTableDistribution, 1, "Distribution" )  }     ; no init needed
  ; ----------------------------------------------------------------------
  ; Section_Aa rules to create individual Distribution table rows
         ; SAC 8/6/19 - prevent writing rows for AlteredVerified Existing equipment (that is documented below) (tic #1118)
   "Create cf1rtblRowDist for all HVACDist instances"                         ; fixed logic implemented above (8/6/19 - tic #1118) that was preventing NonAlt-NonVerif-Existing duct systems from being reported here - SAC 04/12/21
      HVACDist:CF1R_DistRow   = {   if (IfValidAnd( FloorAreaServed > 0 )==0) then  UNCHANGED
                                    else if (IsAltered .AND. IsVerified .AND. IsExisting .AND.
                                             IfValidAnd( exFloorAreaServed > FloorAreaServed )) then  UNCHANGED
                                    else  CreateComp( cf1rtblRowDist, Name + "HVACDistRow" )  endif endif  }

  ; Rules to populate New HVACDist table rows for ALL HVACDist instances
      "Set HVACDist:CF1R_DistRow:Aa01_SpaceConditioningDistributionSystemName" HVACDist:CF1R_DistRow:Aa01_SpaceConditioningDistributionSystemName  = {  Name  }
      "Set HVACDist:CF1R_DistRow:Aa02_DuctLocation" HVACDist:CF1R_DistRow:Aa02_DuctLocation  = {  
																				switch(TypeAbbrevStr)  
																					case "DuctsAttic"    : "UnconditionedAttic"
																					case "DuctsCrawl"    : "UnconditionedCrawlSpace" 
																					case "DuctsGarage"   : "UnconditionedGarage"
																					case "DuctsInEx12"   : "ConditionedSpaceExcept12ft"
																					case "DuctsInAll"    : "ConditionedSpace" 
																					case "DuctsNone"     : "Ductless" 
																					case "DuctsOutdoor"  : "Outdoors"
																					case "LowLlCod"      : "ConditionedSpaceDuctsVLL"
																					case "DuctsMult"     : "DuctsInMultipleLocations"
																					default : UNDEFINED
																				endswitch  }
      "Set HVACDist:CF1R_DistRow:Aa03_DuctDesignType" HVACDist:CF1R_DistRow:Aa03_DuctDesignType  =    ; SAC 5/23/19 - switched logic from IsVerified -> DuctDesign
         {  if (IfValidAnd( DuctDesign > 0 )) then "true" else "false" endif  }

      ; SAC 4/24/19 - modified to set enums rather than numeric      ; SAC 2/12/20 - added logic to handle reporting of Existing+New where DuctInsRvalue is already converted to weighted avg of New & Existing R-value (tic #1200)
      "Set HVACDist:CF1R_DistRow:Aa04Supply_DuctRValue" HVACDist:CF1R_DistRow:Aa04Supply_DuctRValue  =    ; {  atof( Format( "%.2f", DuctInsRvalue ) )  }
         {  if (IfValidAnd( DuctsPresent > 0 ) .AND. IfValidAnd( DuctDesign > 0 )) then  "RvalueVerifiedDuctSystem"
            else if (IfValidAnd( DuctInsRvalue >= 0 )==0) then  UNDEFINED
            else  switch (DuctInsRvalue)
                     case   0 :  "R0.0"
                     case 2.1 :  "R2.1"
                     case 4.2 :  "R4.2"
                     case   6 :  "R6"
                     case   8 :  "R8"
                     case  10 :  "R10"
                     case  12 :  "R12"
                     default  :  if ((Proj:RunScope == 2 .OR. Proj:IsAddAlone > 0.5) .AND. Status == 4 .AND. LocalStatus( exDuctInsRvalue ) > 0 .AND. FloorAreaServed > 0 )
                                 then  switch (DuctInsRvalOpt)
                                          case    0 :  "R0.0"
                                          case  210 :  "R2.1"
                                          case  420 :  "R4.2"
                                          case  600 :  "R6"
                                          case  800 :  "R8"
	     	                                 case 1000 :  "R10"
	     	                                 case 1200 :  "R12"
	     	                                 default   :  UNDEFINED
                                       endswitch
                                 else  UNDEFINED  endif
                  endswitch  endif endif  }
      ; RJH 1/16/2020 - added NotApplicableMessage per ticket #1184
      "Set HVACDist:CF1R_DistRow:Aa04Supply_NotApplicableMessage" HVACDist:CF1R_DistRow:Aa04Supply_NotApplicableMessage  =
         {  if ( LocalStatus ( CF1R_DistRow:Aa04Supply_DuctRValue ) < 1 ) then "n/a" else UNDEFINED endif  }
      "Set HVACDist:CF1R_DistRow:Aa04bSupply_DuctRValueNum" HVACDist:CF1R_DistRow:Aa04bSupply_DuctRValueNum  =    ; SAC 5/23/19 - added numeric version of supply duct R-value to handle DuctDesign runs
         {  if (IfValidAnd( DuctsPresent > 0 ) .AND. IfValidAnd( DuctDesign > 0 )) then  atof( Format( "%.2f", DuctInsRvalue ) )
            else if (IfValidAnd( DuctInsRvalue >= 0 )==0) then  UNDEFINED
            else  atof( Format( "%.2f", DuctInsRvalue ) )  endif endif  }
      ; RJH 1/16/2020 - added NotApplicableMessage per ticket #1184
      "Set HVACDist:CF1R_DistRow:Aa04bSupply_NotApplicableMessage" HVACDist:CF1R_DistRow:Aa04bSupply_NotApplicableMessage  =
         {  if ( LocalStatus ( CF1R_DistRow:Aa04bSupply_DuctRValueNum ) < 1 ) then "n/a" else UNDEFINED endif  }

      "Set HVACDist:CF1R_DistRow:Aa05Return_DuctRValue" HVACDist:CF1R_DistRow:Aa05Return_DuctRValue  =    ; {  atof( Format( "%.2f", RetDuctInsRvalue ) )  }     ; SAC 4/18/19 - DuctInsRvalue -> RetDuctInsRvalue
         {  if (IfValidAnd( DuctsPresent > 0 ) .AND. IfValidAnd( DuctDesign > 0 )) then  "RvalueVerifiedDuctSystem"
            else if (IfValidAnd( RetDuctInsRvalue >= 0 )==0) then  UNDEFINED
            else  switch (RetDuctInsRvalue)
                     case   0 :  "R0.0"
                     case 2.1 :  "R2.1"
                     case 4.2 :  "R4.2"
                     case   6 :  "R6"
                     case   8 :  "R8"
                     case  10 :  "R10"
                     case  12 :  "R12"
                     default  :  UNDEFINED
                  endswitch  endif endif  }

      ; RJH 1/16/2020 - added NotApplicableMessage per ticket #1184
      "Set HVACDist:CF1R_DistRow:Aa05Return_NotApplicableMessage" HVACDist:CF1R_DistRow:Aa05Return_NotApplicableMessage  =
         {  if ( LocalStatus ( CF1R_DistRow:Aa05Return_DuctRValue ) < 1 ) then "n/a" else UNDEFINED endif  }
      "Set HVACDist:CF1R_DistRow:Aa05bReturn_DuctRValueNum" HVACDist:CF1R_DistRow:Aa05bReturn_DuctRValueNum  =    ; SAC 5/23/19 - added numeric version of supply duct R-value to handle DuctDesign runs
         {  if (IfValidAnd( DuctsPresent > 0 ) .AND. IfValidAnd( DuctDesign > 0 )) then  atof( Format( "%.2f", RetDuctInsRvalue ) )
            else if (IfValidAnd( RetDuctInsRvalue >= 0 )==0) then  UNDEFINED
            else  atof( Format( "%.2f", RetDuctInsRvalue ) )  endif endif  }
      ; RJH 1/16/2020 - added NotApplicableMessage per ticket #1184
      "Set HVACDist:CF1R_DistRow:Aa05bReturn_NotApplicableMessage" HVACDist:CF1R_DistRow:Aa05bReturn_NotApplicableMessage  =
         {  if ( LocalStatus ( CF1R_DistRow:Aa05bReturn_DuctRValueNum ) < 1 ) then "n/a" else UNDEFINED endif  }

      "Set HVACDist:CF1R_DistRow:Aa06Supply_DuctSpaceType" HVACDist:CF1R_DistRow:Aa06Supply_DuctSpaceType  = { 
																											switch(EnumString( SupplyRptLocation ))  
																												case "Attic"    : "Attic"
																												case "Garage"    : "Garage" 
																												case "Conditioned Zone"   : "ConditionedZone"
																												case "Unconditioned Zone"   : "UnconditionedZone"
																												case "Crawl Space"    : "CrawlSpace" 
																												case "Outside"     : "Outside" 
																												case "Multiple"  : "Multiple"
																												default : UNDEFINED
																											endswitch  }
      "Set HVACDist:CF1R_DistRow:Aa07Return_DuctSpaceType" HVACDist:CF1R_DistRow:Aa07Return_DuctSpaceType  = { 
																											switch(EnumString( ReturnRptLocation ))  
																												case "Attic"    : "Attic"
																												case "Garage"    : "Garage" 
																												case "Conditioned Zone"   : "ConditionedZone"
																												case "Unconditioned Zone"   : "UnconditionedZone"
																												case "Crawl Space"    : "CrawlSpace" 
																												case "Outside"     : "Outside" 
																												case "Multiple"  : "Multiple"
																												default : UNDEFINED
																											endswitch  }

      ; SAC 4/30/20 - switched references HVACDist:SupDuctSurfaceArea and RetDuctSurfaceArea to SupplyDuctArea and ReturnDuctArea
      "Set HVACDist:CF1R_DistRow:Aa08Supply_DuctSurfaceArea" HVACDist:CF1R_DistRow:Aa08Supply_DuctSurfaceArea  =        ; SAC 8/2/19 - added logic to leave undefined (& set new N/A proeprty) if not a DuctDesign scenario (tic #1118)
         {  if (IfValidAnd( DuctsPresent > 0 ) .AND. IfValidAnd( DuctDesign > 0 )) then  atof( Format( "%.2f", ValidOr( SupplyDuctArea, 0 ) ) )
            else  UNDEFINED  endif  }
      "Set HVACDist:CF1R_DistRow:Aa08Supply_NotApplicableMessage" HVACDist:CF1R_DistRow:Aa08Supply_NotApplicableMessage  =
         {  if ( LocalStatus ( CF1R_DistRow:Aa08Supply_DuctSurfaceArea ) < 1 ) then "n/a" else UNDEFINED endif  }

      "Set HVACDist:CF1R_DistRow:Aa09Return_DuctSurfaceArea" HVACDist:CF1R_DistRow:Aa09Return_DuctSurfaceArea  =        ; SAC 8/2/19 - added logic to leave undefined (& set new N/A proeprty) if not a DuctDesign scenario (tic #1118)
         {  if (IfValidAnd( DuctsPresent > 0 ) .AND. IfValidAnd( DuctDesign > 0 )) then  atof( Format( "%.2f", ValidOr( ReturnDuctArea, 0 ) ) )
            else  UNDEFINED  endif  }
      "Set HVACDist:CF1R_DistRow:Aa09Return_NotApplicableMessage" HVACDist:CF1R_DistRow:Aa09Return_NotApplicableMessage  =
         {  if ( LocalStatus ( CF1R_DistRow:Aa09Return_DuctSurfaceArea ) < 1 ) then "n/a" else UNDEFINED endif  }

      "Set HVACDist:CF1R_DistRow:Aa10_HasBypassDuct" HVACDist:CF1R_DistRow:Aa10_HasBypassDuct  = {  if (HasBypassDuct) then "true" else "false" endif  }
      "Set HVACDist:CF1R_DistRow:Aa11_DuctLeakageDiagnosticCategory" HVACDist:CF1R_DistRow:Aa11_DuctLeakageDiagnosticCategory  = {  
																														switch( EnumString( DuctLeakage ) )
																															case "Sealed and tested"         : "DuctsSealedTested"
																															case "Existing (not specified)"  : "Existing"
																															default : UNDEFINED
																														endswitch  }
      "Set HVACDist:CF1R_DistRow:Aa12_DistributionHERS_VerificationName" HVACDist:CF1R_DistRow:Aa12_DistributionHERS_VerificationName  = {  
            if (LocalCompAssigned( HERSCheck ) < 1) then  UNDEFINED
            else  HERSCheck:Name  endif  }
      "Set HVACDist:CF1R_DistRow:Aa12_NotApplicableMessage" HVACDist:CF1R_DistRow:Aa12_NotApplicableMessage  = { if ( LocalStatus ( CF1R_DistRow:Aa12_DistributionHERS_VerificationName ) < 1 ) then "n/a" else UNDEFINED endif  }

      "Set HVACDist:CF1R_DistRow:Aa13_StatusEAA" HVACDist:CF1R_DistRow:Aa13_StatusEAA  =        ; SAC 7/30/19 - added logic to report Aa13_StatusEAA for AdditionAlone projects (tic #1118)  /  SAC 8/2/19 - logic added for ExistingPlusNew (tic #1118)
         {  if (Proj:RunScope = 1 .AND. IfValidAnd( Proj:IsAddAlone != 0 )==0)   ; new construction
            then  UNDEFINED
            else if (IsNew)
            then  "New"  
            else if (Status == 4)
            then  "ExistingPlusNew"
            else if (IsExisting)
            then  "ExistingUnchanged"
            else if (IsAltered)
            then  "ExistingAlteration"
            else  UNDEFINED   ; error?
            endif endif endif endif endif  }
      "Set HVACDist:CF1R_DistRow:Aa14_VerifiedExistingCondition"
         HVACDist:CF1R_DistRow:Aa14_VerifiedExistingCondition  = {
            if (Proj:RunScope = 1)   ; new construction
            then  UNDEFINED
            else if (IsNew)
            then  "NA"
            else if (IsAltered .AND. IsVerified)
            then  "Yes"
            else  "No"
            endif endif endif  }

; RJH - 20190425 - is there an existing system name?
;      "Set HVACDist:CF1R_DistRow:Aa15_SpaceConditioningDistributionSystemName" HVACDist:CF1R_DistRow:Aa15_SpaceConditioningDistributionSystemName  = {  exName  }

    "Set HVACDist:CF1R_DistRow:Aa16_Installing40PlusLinearFeetDucts"    HVACDist:CF1R_DistRow:Aa16_Installing40PlusLinearFeetDucts  =     ; SAC 5/20/19 - RC5 issues
      {  if (Proj:RunScope == 2 .AND. IfValidAnd( Status = 4 ) .AND.
             IfValidAnd( IsLessThanFortyFt > 0 )==0)
         then  "true"  else  "false"  endif  }

  ; Rule to add Distribution table rows to the cf1rTableDistribution
      "Set HVACDist:CF1R_DistRow:Parent"
         HVACDist:CF1R_DistRow:Parent  = {  cf1rTableDistribution:Name  }
