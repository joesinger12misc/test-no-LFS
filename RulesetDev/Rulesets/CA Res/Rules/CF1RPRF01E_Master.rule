;*************************************************************************
RULELIST "CF1RPRF01E_Final_SingleOrientationEUI"  1 0 0 1

  ; Create cf1rSingleOrientationEUI object
   "Create & initialize new cf1rSingleOrientationEUI object"
      cf1rSection_EnergyUseIntensity:Action   = {  CreateChildren( cf1rSingleOrientationEUI, 1, "SingleOrientationEUI" )  }     ; no init needed

   "Set cf1rSingleOrientationEUI:EuiGrossRowLabel"  cf1rSingleOrientationEUI:EuiGrossRowLabel  = {  "Gross EUI"  }
; Standard Gross EUI= (((Standard Total Energy Use [kWh] +     |PV|    +   |Flex|)    * 3.412) + (Standard Total Energy Use [therms] * 100)) / Building Total Floor Area
; Standard Gross EUI= (((           Enduse10[5]          - Enduse13[5] - Enduse98[5]) * 3.412) + (           Enduse10[6]             * 100)) / Proj:CondFloorArea
   "Set cf1rSingleOrientationEUI:EuiGross01_StandardDesignGrossEUI"  cf1rSingleOrientationEUI:EuiGross01_StandardDesignGrossEUI  = { atof( Format( "%.2f",
        ( ( ( StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse10[5], "0")) - StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse13[5], "0")) - StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse98[5], "0")) ) * 3.412 ) 
        + ( StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse10[6], "0")) * 100 ) ) / Proj:CondFloorArea ) )  }

; Proposed Gross EUI= (((Proposed Total Energy Use [kWh] +     |PV|    +   |Flex|)    * 3.412) + (Proposed Total Energy Use [therms] * 100)) / Building Total Floor Area
; Proposed Gross EUI= (((           Enduse10[2]          - Enduse13[2] - Enduse98[2]) * 3.412) + (           Enduse10[3]             * 100)) / Proj:CondFloorArea
   "Set cf1rSingleOrientationEUI:EuiGross02_ProposedDesignGrossEUI"  cf1rSingleOrientationEUI:EuiGross02_ProposedDesignGrossEUI  = { atof( Format( "%.2f",
        ( ( ( StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse10[2], "0")) - StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse13[2], "0")) - StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse98[2], "0")) ) * 3.412 ) 
        + ( StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse10[3], "0")) * 100 ) ) / Proj:CondFloorArea ) )  }

; Gross EUI Margin
   "Set cf1rSingleOrientationEUI:EuiGross03_MarginGrossEUI"  cf1rSingleOrientationEUI:EuiGross03_MarginGrossEUI  = { atof( Format( "%.2f", EuiGross01_StandardDesignGrossEUI - EuiGross02_ProposedDesignGrossEUI ) ) }

; Gross EUI Margin Percent
   "Set cf1rSingleOrientationEUI:EuiGross04_MarginPctGrossEUI"  cf1rSingleOrientationEUI:EuiGross04_MarginPctGrossEUI  = { atof( Format( "%.2f", 100 * EuiGross03_MarginGrossEUI / EuiGross01_StandardDesignGrossEUI ) ) }
       
   "Set cf1rSingleOrientationEUI:EuiNetRowLabel"  cf1rSingleOrientationEUI:EuiNetRowLabel  = {  "Net EUI"  }
; Standard Net EUI  = ((Standard Total Energy Use [kWh] * 3.412) + (Standard Total Energy Use [therms] * 100)) / Building Total Floor Area
; Standard Net EUI  = ((            Enduse10[5]         * 3.412) + (           Enduse10[6]             * 100)) / Proj:CondFloorArea
   "Set cf1rSingleOrientationEUI:EuiNet01_StandardDesignNetEUI"  cf1rSingleOrientationEUI:EuiNet01_StandardDesignNetEUI  = { atof( Format( "%.2f",
        ( ( StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse10[5], "0")) * 3.412 ) 
        + ( StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse10[6], "0")) * 100 ) ) / Proj:CondFloorArea ) )  }

; Proposed Net EUI  = ((Proposed Total Energy Use [kWh] * 3.412) + (Proposed Total Energy Use [therms] * 100)) / Building Total Floor Area
; Proposed Net EUI  = (((           Enduse10[2]         * 3.412) + (           Enduse10[3]             * 100)) / Proj:CondFloorArea
   "Set cf1rSingleOrientationEUI:EuiNet02_ProposedDesignNetEUI"  cf1rSingleOrientationEUI:EuiNet02_ProposedDesignNetEUI  = { atof( Format( "%.2f",
        ( ( StrToFlt(ValidOr(Proj:ResultSummary[1]:Enduse10[2], "0")) * 3.412 ) 
