   "Set DHWSys:CF1R_DWHRSFRow[1]:Tdw01_ResidentialDwellingUnitDescriptor"   DHWSys:CF1R_DWHRSFRow[1]:Tdw01_ResidentialDwellingUnitDescriptor  = {  "Dwelling"  }
