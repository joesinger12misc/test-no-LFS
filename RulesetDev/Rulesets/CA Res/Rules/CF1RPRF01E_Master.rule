RULELIST "CF1RPRF01E_TableDWHR_MFam_DHWSys1_DWHR1"  1 0 0 1
   "Set DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw01_ResidentialDwellingUnitDescriptor"   DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw01_ResidentialDwellingUnitDescriptor  = {  Name  }
   "Set DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw02_DWHR_Name"                           DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw02_DWHR_Name                          = {  Format( "%s - 1 - %s", DHWSysRef[1]:Name, DWHRSysRef1[1]:Name )  }
   "Set DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw03_DWHR_Configuration"                  DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw03_DWHR_Configuration                 = {  
         if (     IfValidAnd( DWHRSysTakeCold1[1] > 0 )==0 .AND. IfValidAnd( DWHRSysFeedHeater1[1] > 0 )   ) then  "UnequalFlowWaterHeater"
         else if (IfValidAnd( DWHRSysTakeCold1[1] > 0 )    .AND. IfValidAnd( DWHRSysFeedHeater1[1] > 0 )==0) then  "UnequalFlowShower"
         else  "EqualFlow"  endif endif  }
   "Set DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw04_DWHR_DrainCount"                     DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw04_DWHR_DrainCount                    = {  DWHRSysShowers1[1]  }
   "Set DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw05_DWHR_RatedEffectiveness"             DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw05_DWHR_RatedEffectiveness            = {  DWHRSysRef1[1]:CSARatedEff  }
   "Set DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw06_DWHR_HERS_VerificationRequired"      DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw06_DWHR_HERS_VerificationRequired     = {  "true"  }
   "Set DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw07_DWHR_PercentShowerServed"            DwellUnitType:CF1R_DWHRMFRow_DWHR1[1]:Tdw07_DWHR_PercentShowerServed           = {  
         if (     IfValidAnd( DWHRSysTakeCold1[1] > 0 )    .AND. IfValidAnd( DWHRSysFeedHeater1[1] > 0 )==0)
         then  100 * DWHRSysTakeCold1[1] / TotShowersServed[1]
         else  100 * DWHRSysShowers1[1]  / TotShowersServed[1]  endif  }
END
