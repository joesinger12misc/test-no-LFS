;*************************************************************************
RULELIST "CF1RPRF01E_Final"  1 0 0 1
  ; ----------------------------------------------------------------------
  ; Rules to create CF1RPRF01E objects populated w/ Final analysis results



  ; Section_C Manual Field population
     "Set cf1rSection_C:C01_ResidentialPerformanceComplianceResult"  cf1rSection_C:C01_ResidentialPerformanceComplianceResult  = {  
          if ( Project:ResultSummary[1]:PassFail == "PASS" )
          then "true"
          else "false"
          endif }

  ; Section_EnergyUseSummary
   "Call subordinate rulelist to handle single vs. multiple orientation results"
      Project:Action   = {  if (AllOrientations == 0) 
                            then if (Proj:AnalysisVersion > 220000) 
                                 then EvalRulelist( "CF1RPRF01E_Final_SingleOrientationEUS" )
                                 else EvalRulelist( "CF1RPRF01E_Final_SingleOrientation" )
                                 endif
                            else if (Proj:AnalysisVersion > 220000) 
                                 then EvalRulelist( "CF1RPRF01E_Final_MultipleOrientationsEUS" )
                                 else EvalRulelist( "CF1RPRF01E_Final_MultipleOrientations" )
                                 endif   
                            endif }


  ; Section_EnergyUseIntensity
   "Call subordinate rulelist to handle single vs. multiple orientation results"
      Project:Action   = {  if (Proj:AnalysisVersion > 220000) 
                            then if (AllOrientations == 0)
                                 then EvalRulelist( "CF1RPRF01E_Final_SingleOrientationEUI" )
                                 else EvalRulelist( "CF1RPRF01E_Final_MultipleOrientationEUI" )
                                 endif
                            else UNCHANGED
                            endif }


  ; Section_Umlh
   "Create and Populate Section_Umlh Single/Multiple results objects"
      cf1rSection_Umlh:Action  = {  if (IfValidAnd( Proj:AllOrientations > 0 ))
      	                            then  EvalRulelist( "CF1RPRF01E_Section_Umlh_MultipleOrientations_Populate" )
	                                else  EvalRulelist( "CF1RPRF01E_Section_Umlh_SingleOrientation_Populate" )
	                                endif }


  ; Section_EnergyDesignRatings
   "Create Section_EnergyDesignRatings Single/Multiple results objects"
      cf1rSection_EnergyDesignRatings:Action  = {  if (GlobalStatus(Proj:ResultSummary[1]:DesignRating) > 0) 
	                                               then  if (IfValidAnd( Proj:AllOrientations > 0 ))
      	                                                 then  CreateChildren( cf1rEDRMultipleOrientations, 1, "MultipleOrientations", "CF1RPRF01E_Section_EnergyDesignRatings_MultipleOrientations_Create" )
	                                                     else  CreateChildren( cf1rEDRSingleOrientation   , 1, "SingleOrientation"   , "CF1RPRF01E_Section_EnergyDesignRatings_SingleOrientation_Create"    )
	                                                     endif
                                                   else  UNCHANGED  endif }
   "Populate Section_EnergyDesignRatings Single/Multiple results objects"
      cf1rSection_EnergyDesignRatings:Action  = {  if (ComponentCount( cf1rEDRMultipleOrientations ) > 0) 
	                                                then  EvalRulelist( "CF1RPRF01E_Section_EnergyDesignRatings_MultipleOrientations_Populate" )
	                                                else if (ComponentCount( cf1rEDRSingleOrientation ) > 0) 
	                                                then  EvalRulelist( "CF1RPRF01E_Section_EnergyDesignRatings_SingleOrientation_Populate"    )
	                                                else  UNCHANGED  endif endif }


  ; Section_Da Energy Design Rating PV SYSTEM INPUTS - SIMPLIFIED (NOTE - should be created within RULELIST "CF1RPRF01E_Proposed" to be located correctly, then populated here)
   "Call subordinate rulelist to populate Section_Da"       ; SAC 2/3/20 - revised rule to call rulelist to only UPDATE PV reporting records here, based on final (scaled) PV sizes (tic #1012)
      Project:Action   = { if (ComponentCount( cf1rSection_Da ) > 0)
                           then  EvalRulelist( "CF1RPRF01E_Section_Da_FinalUpdate" )
                           else  UNCHANGED  endif }

  ; Section_Db Energy Design Rating BATTERY INPUTS (NOTE - should be created within RULELIST "CF1RPRF01E_Proposed" to be located correctly, then populated here)
;   "Create & initialize new cf1rSection_Db object"
;      cf1rCF1RPRF01E:Action  = {  
;	       if (Proj:SimulateBattery > 0)
;		   then CreateChildren( cf1rSection_Db, 1, "Section_Db" )
;		   else UNCHANGED
;		   endif  }
   "Call subordinate rulelist to populate Section_Db"
      Project:Action   = {  if (IfValidAnd( Proj:SimulateBattery > 0 )) 
	                        then  EvalRulelist( "CF1RPRF01E_Section_Db" )
                            else  UNCHANGED  endif }

   "Call subordinate rulelist to populate final calculated SSF (Tsh10_SolarWaterHeatingSavingsFraction)"     ; SAC 2/4/20 (tic #1198)
      Proj:Action  = {  if (ComponentCount( cf1rTableWaterHeatingSolar ) < 1) then  UNCHANGED
                        else  EvalRulelist( "CF1RPRF01E_TableWaterHeatingSolar_SSF" )  endif  }

;     cf1rSection_E  (child of cf1rCF1RPRF01E)
   "Add/adjust Special Features messages via separate rulelist"     ; SAC 10/13/18
      Project:Action  = {  EvalRulelist( "CF1RPRF01E_Final_SpecialFeatures" )  }

;     cf1rSection_Ea  (child of cf1rCF1RPRF01E)
   "Add/adjust HERS Features messages via separate rulelist"     ; SAC 10/15/18
      Project:Action  = {  EvalRulelist( "CF1RPRF01E_Final_HERSFeatures" )  }

END
;*************************************************************************




