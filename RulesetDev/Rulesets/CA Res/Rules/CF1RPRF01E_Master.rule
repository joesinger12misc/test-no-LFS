      {  if (Proj:RunScope = 1 .AND. IfValidAnd( Proj:IsAddAlone != 0 )==0)   ; new construction
         then  UNDEFINED
         else if (IsNew)
         then  "New"  
         else if (IsExisting)
         then  "Existing"
         else if (IsAltered)
         then  "Altered"
         else  UNDEFINED   ; error?
         endif endif endif endif  }
