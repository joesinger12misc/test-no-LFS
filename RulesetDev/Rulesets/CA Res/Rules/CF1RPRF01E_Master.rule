   "Set DHWSys:CF1R_DWHRSFRow[1]:Tdw03_DWHR_Configuration"                  DHWSys:CF1R_DWHRSFRow[1]:Tdw03_DWHR_Configuration                 = {  
         if (     IfValidAnd( DWHRSysTakeCold[1] > 0 )==0 .AND. IfValidAnd( DWHRSysFeedHeater[1] > 0 )   ) then  "UnequalFlowWaterHeater"
         else if (IfValidAnd( DWHRSysTakeCold[1] > 0 )    .AND. IfValidAnd( DWHRSysFeedHeater[1] > 0 )==0) then  "UnequalFlowShower"
         else  "EqualFlow"  endif endif  }
   "Set DHWSys:CF1R_DWHRSFRow[1]:Tdw04_DWHR_DrainCount"                     DHWSys:CF1R_DWHRSFRow[1]:Tdw04_DWHR_DrainCount                    = {  DWHRSysShowers[1]  }
