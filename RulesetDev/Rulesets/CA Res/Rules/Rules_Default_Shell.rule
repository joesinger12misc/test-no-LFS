	"Set DHWSys:TotNumDHWHeaters - 4"	DHWSys:TotNumDHWHeaters = {	if (LocalCompAssigned( DHWHeater[4]  )) then  TotNumDHWHeaters + HeaterMultFlt[4]   else  UNCHANGED  endif  }
	"Set DHWSys:TotNumDHWHeaters - 5"	DHWSys:TotNumDHWHeaters = {	if (LocalCompAssigned( DHWHeater[5]  )) then  TotNumDHWHeaters + HeaterMultFlt[5]   else  UNCHANGED  endif  }
	"Set DHWSys:TotNumDHWHeaters - 6"	DHWSys:TotNumDHWHeaters	= {	if (LocalCompAssigned( DHWHeater[6]  )) then  TotNumDHWHeaters + HeaterMultFlt[6]   else  UNCHANGED  endif  }
