      {  if (MinIAQCFM <= 0 .OR. IfValidAnd( DwellUnitTypeRef:IAQDisabled > 0.5 )) then  0
         else  ValidOr( DwellUnitTypeRef:StdDesignIAQCFM, 0 ) * Count  endif  }
