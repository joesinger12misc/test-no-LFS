																															      else	UnitClVentZone[4]:ClVentAirFlow[1] + UnitClVentCFM[4]  endif  }
	"Set Proj:UnitClVentZone[4]:ClVentPower[1]"		Proj:UnitClVentZone[4]:ClVentPower[1]		= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[4]:ExhaustsTo = 1 )==0) then  UNCHANGED
																															      else  UnitClVentZone[4]:ClVentPower[1] + (UnitClVentFan[4]:WperCFMCool * UnitClVentCFM[4])  endif  }
	"Set Proj:UnitClVentZone[4]:ClVentAirFlow[2]"	Proj:UnitClVentZone[4]:ClVentAirFlow[2]	= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[4]:ExhaustsTo = 2 )==0) then  UNCHANGED
																															      else	UnitClVentZone[4]:ClVentAirFlow[2] + UnitClVentCFM[4]  endif  }
