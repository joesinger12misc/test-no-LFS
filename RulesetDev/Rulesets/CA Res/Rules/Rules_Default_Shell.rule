RULELIST "Set_ZoneClVent_SFam"  0 0 0 0
;*************************************************************************
	"Init Zone:ClVentAirFlow[1]"			Zone:ClVentAirFlow[1]	= {	0  }     ; SAC 4/23/19 - expanded all rules for separate tracking of ClVent to attic vs. outside (tic #1023)
	"Init Zone:ClVentAirFlow[2]"			Zone:ClVentAirFlow[2]	= {	0  }
	"Init Zone:ClVentPower[1]"				Zone:ClVentPower[1]		= {	0  }
	"Init Zone:ClVentPower[2]"				Zone:ClVentPower[2]		= {	0  }

   ; SAC 1/20/19 - updated rules for new UnitClVentOption = 'CFI (Central Fan Integrated) Cool Vent' option (tic #1040)
   ; SAC 10/23/13 - fixed bug where UnitClVentZone[2-6]:ClVentPower were summing previous ClVentAirFlow rather than ClVentPower from earlier rules (GC issue #217)
	; SAC 4/23/19 - revisions to track airflow & power separately for cool vent to attic (1) vs. outside (2) (tic #1023)
	"Set Proj:UnitClVentZone[1]:ClVentAirFlow[1]-2attic"	Proj:UnitClVentZone[1]:ClVentAirFlow[1]	= {
	      if (ClVentUI < 0.5) then  UNCHANGED
	      else if (UnitClVentOption == 1 .OR. (UnitClVentOption == 2 .AND. IfValidAnd( UnitClVentFan[1]:ExhaustsTo = 1 ))) then  UnitClVentCFM[1]
	      else  UNCHANGED  endif endif  }
	"Set Proj:UnitClVentZone[1]:ClVentAirFlow[2]-2outside"	Proj:UnitClVentZone[1]:ClVentAirFlow[2]	= {
	      if (ClVentUI < 0.5) then  UNCHANGED
	      else if (UnitClVentOption == 5 .OR. (UnitClVentOption == 2 .AND. IfValidAnd( UnitClVentFan[1]:ExhaustsTo = 2 ))) then  UnitClVentCFM[1]
	      else  UNCHANGED  endif endif  }
   ; SAC 2/12/18 - replaced 'hard-coded' 0.1 W/CFM default WHF power value w/ look-up table entry to accommodate 2019 update (tic #977)
	"Set Proj:UnitClVentZone[1]:ClVentPower[1]-2attic"		Proj:UnitClVentZone[1]:ClVentPower[1]		= {
         if (ClVentUI < 0.5) then  UNCHANGED
         else if (UnitClVentOption == 1) then	UnitClVentCFM[1] *
                     T24RClimateZoneCodeBase:WHFWperCFM( "ClimateZone", Proj:ClimateZone, "CodeBase", Proj:StdDesignBase, "BldgType", Proj:BldgTypeTblStr )
         else if (UnitClVentOption == 2 .AND. IfValidAnd( UnitClVentFan[1]:ExhaustsTo = 1 ))
         then  UnitClVentFan[1]:WperCFMCool * UnitClVentCFM[1]
         else  UNCHANGED  endif endif endif  }
	"Set Proj:UnitClVentZone[1]:ClVentPower[2]-2outside"		Proj:UnitClVentZone[1]:ClVentPower[2]		= {
         if (ClVentUI < 0.5) then  UNCHANGED
         else if (UnitClVentOption == 5) then	UnitClVentCFM[1] *
                     T24RClimateZoneCodeBase:WHFWperCFM( "ClimateZone", Proj:ClimateZone, "CodeBase", Proj:StdDesignBase, "BldgType", Proj:BldgTypeTblStr )
         else if (UnitClVentOption == 2 .AND. IfValidAnd( UnitClVentFan[1]:ExhaustsTo = 2 ))
         then  UnitClVentFan[1]:WperCFMCool * UnitClVentCFM[1]
         else  UNCHANGED  endif endif endif  }

	"Set Proj:UnitClVentZone[2]:ClVentAirFlow[1]"	Proj:UnitClVentZone[2]:ClVentAirFlow[1]	= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[2]:ExhaustsTo = 1 )==0) then  UNCHANGED
																															      else	UnitClVentZone[2]:ClVentAirFlow[1] + UnitClVentCFM[2]  endif  }
	"Set Proj:UnitClVentZone[2]:ClVentPower[1]"		Proj:UnitClVentZone[2]:ClVentPower[1]		= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[2]:ExhaustsTo = 1 )==0) then  UNCHANGED
																															      else  UnitClVentZone[2]:ClVentPower[1] + (UnitClVentFan[2]:WperCFMCool * UnitClVentCFM[2])	endif  }
	"Set Proj:UnitClVentZone[2]:ClVentAirFlow[2]"	Proj:UnitClVentZone[2]:ClVentAirFlow[2]	= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[2]:ExhaustsTo = 2 )==0) then  UNCHANGED
																															      else	UnitClVentZone[2]:ClVentAirFlow[2] + UnitClVentCFM[2]  endif  }
	"Set Proj:UnitClVentZone[2]:ClVentPower[2]"		Proj:UnitClVentZone[2]:ClVentPower[2]		= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[2]:ExhaustsTo = 2 )==0) then  UNCHANGED
																															      else  UnitClVentZone[2]:ClVentPower[2] + (UnitClVentFan[2]:WperCFMCool * UnitClVentCFM[2])	endif  }

	"Set Proj:UnitClVentZone[3]:ClVentAirFlow[1]"	Proj:UnitClVentZone[3]:ClVentAirFlow[1]	= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[3]:ExhaustsTo = 1 )==0) then  UNCHANGED
																															      else	UnitClVentZone[3]:ClVentAirFlow[1] + UnitClVentCFM[3]  endif  }
	"Set Proj:UnitClVentZone[3]:ClVentPower[1]"		Proj:UnitClVentZone[3]:ClVentPower[1]		= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[3]:ExhaustsTo = 1 )==0) then  UNCHANGED
																															      else  UnitClVentZone[3]:ClVentPower[1] + (UnitClVentFan[3]:WperCFMCool * UnitClVentCFM[3])  endif  }
	"Set Proj:UnitClVentZone[3]:ClVentAirFlow[2]"	Proj:UnitClVentZone[3]:ClVentAirFlow[2]	= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[3]:ExhaustsTo = 2 )==0) then  UNCHANGED
																															      else	UnitClVentZone[3]:ClVentAirFlow[2] + UnitClVentCFM[3]  endif  }
	"Set Proj:UnitClVentZone[3]:ClVentPower[2]"		Proj:UnitClVentZone[3]:ClVentPower[2]		= {	if (ClVentFanUI < 0.5 .OR. UnitClVentOption != 2 .OR. IfValidAnd( UnitClVentFan[3]:ExhaustsTo = 2 )==0) then  UNCHANGED
																															      else  UnitClVentZone[3]:ClVentPower[2] + (UnitClVentFan[3]:WperCFMCool * UnitClVentCFM[3])  endif  }

