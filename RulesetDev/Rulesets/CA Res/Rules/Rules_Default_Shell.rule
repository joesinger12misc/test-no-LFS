																		case  1 :	3	; "Conditioned"   -> "CZM"
																		case  2 :	3	; "Living"        -> "CZM"
																		case  3 :	3	; "Sleeping"      -> "CZM"
																		case 10 :	4	; "Unconditioned" -> "UZM"
