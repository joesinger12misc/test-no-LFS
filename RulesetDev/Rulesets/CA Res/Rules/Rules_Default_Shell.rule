	"Set DHWSys:TotNumDHWHeaters - 1"	DHWSys:TotNumDHWHeaters	= {	if (Proj:IsMultiFamily > 0 .AND. CentralDHW > 0 .AND. CentralDHWTypeVal == 2) then  1
	                                                             else if (LocalCompAssigned( DHWHeater[1]  )) then                     HeaterMultFlt[1]   else      0      endif endif  }
