ENUM_PROPERTIES_VALUE   ; ensures retrieval/logic/us of enum properties assume numeric value (alternative is ENUM_PROPERTIES_STRING)

;************************************************************************* SAC 06/14/24 (tic #1378)
RULELIST "CUAC_Default_Always" 0 0 0 0
;*************************************************************************

   "default CUAC:SFamAffordableUnitCount"    CUAC:SFamAffordableUnitCount = {  1  }       ; SAC 10/22/24 (tic #1378)
   "default CUAC:SFamMktRateUnitCount"       CUAC:SFamMktRateUnitCount    = {  0  }

   "default CUAC:IncludeDHWInUtilityBills"   CUAC:IncludeDHWInUtilityBills = {  1  }      ; SAC 09/03/24

   "default CUAC:TariffDate"        CUAC:TariffDate      = {  "12/18/2023"  }

;   "default CUAC:ElecUtility
;   "default CUAC:G2ElecTerritory
   "default CUAC:ElecTerritory"     CUAC:ElecTerritory   = {  "- select territory -"  }
   "default CUAC:ElecTariff"        CUAC:ElecTariff      = {  "- select tariff -"  }

;   "default CUAC:GasUtility
;   "default CUAC:G2GasTerritory
   "default CUAC:GasTerritory"      CUAC:GasTerritory    = {  "- select territory -"  }
   "default CUAC:GasTariff"         CUAC:GasTariff       = {  "- select tariff -"  }

   "default CUAC:ElecTariffGen"     CUAC:ElecTariffGen   = 
      {  if (EnumValue( ElecUtility ) < 1 .OR. EnumValue( G2ElecTerritory ) < 1 .OR. EnumValue( G2ElecTariff ) < 1) then  0
         else if (CUAC_UtilityRateGen2Data:WhichGen( "Fuel", "Electric", "Utility", EnumString( ElecUtility ), "Territory", EnumString( G2ElecTerritory ), "Rate", EnumString( G2ElecTariff ) ) > 0)
         then     CUAC_UtilityRateGen2Data:WhichGen( "Fuel", "Electric", "Utility", EnumString( ElecUtility ), "Territory", EnumString( G2ElecTerritory ), "Rate", EnumString( G2ElecTariff ) )
         else if (CUAC_UtilityRates:IsValid( "RateID", EnumValue( G2ElecTariff ) ))
         then  1  else  0  endif endif endif  }
   "default CUAC:GasTariffGen"      CUAC:GasTariffGen    =
      {  if (EnumValue( GasUtility ) < 1 .OR. EnumValue( G2GasTerritory ) < 1 .OR. EnumValue( G2GasTariff ) < 1) then  0
         else if (CUAC_UtilityRateGen2Data:WhichGen( "Fuel", "Gas", "Utility", EnumString( GasUtility ), "Territory", EnumString( G2GasTerritory ), "Rate", EnumString( G2GasTariff ) ) > 0)
         then     CUAC_UtilityRateGen2Data:WhichGen( "Fuel", "Gas", "Utility", EnumString( GasUtility ), "Territory", EnumString( G2GasTerritory ), "Rate", EnumString( G2GasTariff ) )
         else if (CUAC_UtilityRates:IsValid( "RateID", EnumValue( G2GasTariff ) ))
         then  1  else  0  endif endif endif  }

   "default CUAC:ElecTariffIsTOU"   CUAC:ElecTariffIsTOU =
      {  if (ElecTariffGen < 2) then  0
         else  CUAC_UtilityRateGen2Data:IsTOURate( "Fuel", "Electric", "Utility", EnumString( ElecUtility ), "Territory", EnumString( G2ElecTerritory ), "Rate", EnumString( G2ElecTariff ) )
         endif  }
   "default CUAC:ElecTariffDescrip" CUAC:ElecTariffDescrip =
      {  if (ElecTariffGen < 2) then  UNDEFINED
         else  CUAC_UtilityRateGen2Data:RateDescription( "Fuel", "Electric", "Utility", EnumString( ElecUtility ), "Territory", EnumString( G2ElecTerritory ), "Rate", EnumString( G2ElecTariff ) )
               + "  [Source: Clean Power Research, L.L.C. 12/18/2023]"
         endif  }
   "default CUAC:G2ElecTariffERN"   CUAC:G2ElecTariffERN =
      {  if (ElecTariffGen < 2) then  UNDEFINED
         else  CUAC_UtilityRateGen2Data:Gen2EncodedRateName( "Fuel", "Electric", "Utility", EnumString( ElecUtility ), "Territory", EnumString( G2ElecTerritory ), "Rate", EnumString( G2ElecTariff ) )
         endif  }

   "default CUAC:ElecTariffType"    CUAC:ElecTariffType =
      {  if (ElecTariffGen < 2) then  UNCHANGED
         else  if (strlen( EnumString( G2ElecTariff ) ) > 3 .AND. strlen( ValidOr( ElecTariffDescrip, "no" ) ) > 3)
               then  if (find( EnumString( G2ElecTariff ), "CARE" ) > 0 .OR. find( ElecTariffDescrip, "CARE" ) > 0)
                     then  2  else  1
                     endif
               else if (strlen( EnumString( G2ElecTariff ) ) > 3)
               then  if (find( EnumString( G2ElecTariff ), "CARE" ) > 0)
                     then  2  else  1
                     endif
               else  1
               endif endif
         endif  }

   "default CUAC:GasTariffIsTOU"    CUAC:GasTariffIsTOU =
      {  if (GasTariffGen < 2) then  0
         else  CUAC_UtilityRateGen2Data:IsTOURate( "Fuel", "Gas", "Utility", EnumString( GasUtility ), "Territory", EnumString( G2GasTerritory ), "Rate", EnumString( G2GasTariff ) )
         endif  }
   "default CUAC:GasTariffDescrip"  CUAC:GasTariffDescrip =
      {  if (GasTariffGen < 2) then  UNDEFINED
         else  CUAC_UtilityRateGen2Data:RateDescription( "Fuel", "Gas", "Utility", EnumString( GasUtility ), "Territory", EnumString( G2GasTerritory ), "Rate", EnumString( G2GasTariff ) )
               + "  [Source: Clean Power Research, L.L.C. 12/18/2023]"
         endif  }
   "default CUAC:G2GasTariffERN"    CUAC:G2GasTariffERN =
      {  if (GasTariffGen < 2) then  UNDEFINED
         else  CUAC_UtilityRateGen2Data:Gen2EncodedRateName( "Fuel", "Gas", "Utility", EnumString( GasUtility ), "Territory", EnumString( G2GasTerritory ), "Rate", EnumString( G2GasTariff ) )
         endif  }

   "default CUAC:GasTariffType"     CUAC:GasTariffType =
      {  if (GasTariffGen < 2) then  0
         else  if (EnumValue( G2GasTerritory ) < 1 .OR. EnumValue( G2GasTariff ) < 1) then  0
               else if (strlen( EnumString( G2GasTariff ) ) > 3 .AND. strlen( ValidOr( GasTariffDescrip, "no" ) ) > 3)
               then  if (find( EnumString( G2GasTariff ), "CARE" ) > 0 .OR. find( GasTariffDescrip, "CARE" ) > 0)
                     then  2  else  1
                     endif
               else if (strlen( EnumString( G2GasTariff ) ) > 3)
               then  if (find( EnumString( G2GasTariff ), "CARE" ) > 0)
                     then  2  else  1
                     endif
               else  1
               endif endif endif
         endif  }

;   "default UtilityRate:Company
   "default UtilityRate:Territory"  UtilityRate:Territory = {  "- select territory -"  }
   "default UtilityRate:Rate"       UtilityRate:Rate      = {  "- select tariff -"  }

   "default CUAC:WaterMonthlyCost"  CUAC:WaterMonthlyCost   = {  0  }
   "default CUAC:WaterVolumeCost"   CUAC:WaterVolumeCost    = {  0  }
   "default CUAC:TrashMonthlyCost"  CUAC:TrashMonthlyCost   = {  0  }

   "default CUAC:UnitType"          CUAC:UnitType  = {  "Affordable Housing"  }

   "remove restricted CUAC user inputs if not in research mode"   CUAC:Action =
      {  if (IfValidAnd( Proj:EnableResearchMode > 0 )==0 .AND. 
             ( LocalStatus( ElecBillReconBeginMo ) > 5 .OR. LocalStatus( ElecBillMinMoCharge ) > 5 .OR.
               LocalStatus( ElecBillMinAnnCharge ) > 5 ))
         then  EvalRulelist( "BlastInputs_CUACOptions" )
         else  UNCHANGED  endif  }
   "Default CUAC:ElecBillReconBeginMo"    CUAC:ElecBillReconBeginMo  = {  "Mar"  }        ; SAC 12/14/23
   "Default CUAC:ElecBillMinMoCharge"     CUAC:ElecBillMinMoCharge   = {  -999   }   ; -999 ignore / -1 calced by rate, regardless of NetSurplusCompensation / >=0 this value
   "Default CUAC:ElecBillMinAnnCharge"    CUAC:ElecBillMinAnnCharge  = {     0   }   ; -999 ignore / >-999 this value

END

;************************************************************************* SAC 08/06/22
RULELIST "CUAC_ZoneUnitCounts" 0 0 0 0
;*************************************************************************
; these rules only evaluated if:  IfValidAnd( Proj:AffordableHousing > 0 ) .AND. IfValidAnd( Proj:CUACReport > 0 )

   "default main/fundamental CUAC stuff regardless of New vs. Old CUAC"    Proj:Action = {  EvalRulelist( "CUAC_Default_Always" )  }      ; SAC 06/14/24

   "Set CUAC:CommunitySolarValid"      CUAC:CommunitySolarValid =          ; other comm slr properties remain in Proj - SAC 05/23/24 (tic #1378)
      {  if (IfValidAnd( Proj:UseCommunitySolar > 0 ) .AND.
             IfValidAnd( Proj:CommunitySolarProject != 0 ) .AND.
             IfValidAnd( Proj:InCmntySlrProjTerritory > 0 ))
         then  1
         else  0
         endif  }

   "default CUAC:PctPVAlloc"                 CUAC:PctPVAlloc  = {  100  }     ; SAC 09/06/24 (CUAC)

   "blast user-defined CUAC:AffordablePVDCSysSize"   CUAC:Action  =
      {  if (LocalStatus( AffordablePVDCSysSize ) > 4) then  EvalRulelist( "BlastInputs_CUAC_AffordablePVDCSysSize" )  else  UNCHANGED  endif  }
   "default CUAC:AffordablePVDCSysSize"      CUAC:AffordablePVDCSysSize = {  Proj:PVWDCSysTotal * PctPVAlloc / 100  }

   "default CUAC:Locality"          CUAC:Locality  =
      {  if (GLobalStatus( Proj:City ) < 1) then  UNDEFINED
         else if (find( Proj:City, "," ) > 1)
         then  substr( Proj:City, 0, find( Proj:City, "," )-1 )
         else  Proj:City  endif endif  }

   "set CUAC:OwnerDetailsLbl"       CUAC:OwnerDetailsLbl =
      {  if (LocalStatus( OwnerName ) > 0)
         then  "Owner Details:  " + substr( OwnerName, 0, 20 ) + "..." 
         else  "Owner Details..."  endif  }
   "set CUAC:ContactDetailsLbl"     CUAC:ContactDetailsLbl =
      {  if (LocalStatus( ContactName ) > 0)
         then  "Contact Details:  " + substr( ContactName, 0, 20 ) + "..." 
         else  "Contact Details..."  endif  }

;   "default ResZn:CUAC_IncludeDHW"     ResZn:CUAC_IncludeDHW =       ; moved from CUAC object - SAC 09/27/23
;      {  if (IfValidAnd( AffordableHousing > 0 )) then  1
;         else  UNDEFINED  endif  }
;
;   "Set ResZn:CUACErrorMsg"      ResZn:CUACErrorMsg =       ; SAC 08/07/22
;      {  if (IfValidAnd( AffordableHousing > 0 )==0) then  UNDEFINED
;         else if (NumAffordableUnitTypes < 1)
;         then  "CUAC Error: No Affordable Dwellings present"
;         else if (NumAffordableUnitTypes > 1)
;         then  "CUAC Error: Dwellings must have same # of bedrooms"
;         else if (IfValidAnd( AffordableUnitsByBedrmsWithMult[8] > 0 ))
;         then  "CUAC Error: No CUAC calcs/reporting for >6 bedroom units"
;         else  UNDEFINED  endif endif endif endif  }
;   "Set ResZn:CUACError"   ResZn:CUACError = {  if (LocalStatus( CUACErrorMsg) > 0) then  1  else  UNDEFINED  endif  }

;   "Set CUAC:AffordableUnitsByBedrms[1] - Studio"     CUAC:AffordableUnitsByBedrms[1] = {  SumAll( ResZn:AffordableUnitsByBedrmsWithMult[1] )  }      ; SAC 08/05/22 (CUAC)
;   "Set CUAC:AffordableUnitsByBedrms[2] - 1 Bdrm"     CUAC:AffordableUnitsByBedrms[2] = {  SumAll( ResZn:AffordableUnitsByBedrmsWithMult[2] )  }
;   "Set CUAC:AffordableUnitsByBedrms[3] - 2 Bdrm"     CUAC:AffordableUnitsByBedrms[3] = {  SumAll( ResZn:AffordableUnitsByBedrmsWithMult[3] )  }
;   "Set CUAC:AffordableUnitsByBedrms[4] - 3 Bdrm"     CUAC:AffordableUnitsByBedrms[4] = {  SumAll( ResZn:AffordableUnitsByBedrmsWithMult[4] )  }
;   "Set CUAC:AffordableUnitsByBedrms[5] - 4 Bdrm"     CUAC:AffordableUnitsByBedrms[5] = {  SumAll( ResZn:AffordableUnitsByBedrmsWithMult[5] )  }
;   "Set CUAC:AffordableUnitsByBedrms[6] - 5 Bdrm"     CUAC:AffordableUnitsByBedrms[6] = {  SumAll( ResZn:AffordableUnitsByBedrmsWithMult[6] )  }
;   "Set CUAC:AffordableUnitsByBedrms[7] - 6 Bdrm"     CUAC:AffordableUnitsByBedrms[7] = {  SumAll( ResZn:AffordableUnitsByBedrmsWithMult[7] )  }
;   "Set CUAC:AffordableUnitsByBedrms[8] - >6 Bdrm"    CUAC:AffordableUnitsByBedrms[8] = {  SumAll( ResZn:AffordableUnitsByBedrmsWithMult[8] )  }
   "Set CUAC:AffordableUnitsByBedrms[1] - Studio"     CUAC:AffordableUnitsByBedrms[1] = {  if (IfValidAnd( Proj:NumBedrooms == 0 )) then  SFamAffordableUnitCount  else  0  endif  }    ; SAC 05/10/24 (tic #1378)  ; SAC 10/22/24
   "Set CUAC:AffordableUnitsByBedrms[2] - 1 Bdrm"     CUAC:AffordableUnitsByBedrms[2] = {  if (IfValidAnd( Proj:NumBedrooms == 1 )) then  SFamAffordableUnitCount  else  0  endif  }
   "Set CUAC:AffordableUnitsByBedrms[3] - 2 Bdrm"     CUAC:AffordableUnitsByBedrms[3] = {  if (IfValidAnd( Proj:NumBedrooms == 2 )) then  SFamAffordableUnitCount  else  0  endif  }
   "Set CUAC:AffordableUnitsByBedrms[4] - 3 Bdrm"     CUAC:AffordableUnitsByBedrms[4] = {  if (IfValidAnd( Proj:NumBedrooms == 3 )) then  SFamAffordableUnitCount  else  0  endif  }
   "Set CUAC:AffordableUnitsByBedrms[5] - 4 Bdrm"     CUAC:AffordableUnitsByBedrms[5] = {  if (IfValidAnd( Proj:NumBedrooms == 4 )) then  SFamAffordableUnitCount  else  0  endif  }
   "Set CUAC:AffordableUnitsByBedrms[6] - 5 Bdrm"     CUAC:AffordableUnitsByBedrms[6] = {  if (IfValidAnd( Proj:NumBedrooms == 5 )) then  SFamAffordableUnitCount  else  0  endif  }
   "Set CUAC:AffordableUnitsByBedrms[7] - 6 Bdrm"     CUAC:AffordableUnitsByBedrms[7] = {  if (IfValidAnd( Proj:NumBedrooms == 6 )) then  SFamAffordableUnitCount  else  0  endif  }
   "Set CUAC:AffordableUnitsByBedrms[8] - >6 Bdrm"    CUAC:AffordableUnitsByBedrms[8] = {  if (IfValidAnd( Proj:NumBedrooms >= 7 )) then  SFamAffordableUnitCount  else  0  endif  }
   "Set CUAC:NumAffordableDwellingUnits - 0-6 Bdrm"   CUAC:NumAffordableDwellingUnits =         ; SAC 09/13/22 (CUAC)
      {  AffordableUnitsByBedrms[1] + AffordableUnitsByBedrms[2] + AffordableUnitsByBedrms[3] + AffordableUnitsByBedrms[4] +
         AffordableUnitsByBedrms[5] + AffordableUnitsByBedrms[6] + AffordableUnitsByBedrms[7]  }

   "Set CUAC:MktRateUnitsByBedrms[1] - Studio"     CUAC:MktRateUnitsByBedrms[1] = {  if (IfValidAnd( Proj:NumBedrooms == 0 )) then  SFamMktRateUnitCount  else  0  endif  }    ; SAC 10/22/24 (tic #1378)
   "Set CUAC:MktRateUnitsByBedrms[2] - 1 Bdrm"     CUAC:MktRateUnitsByBedrms[2] = {  if (IfValidAnd( Proj:NumBedrooms == 1 )) then  SFamMktRateUnitCount  else  0  endif  }
   "Set CUAC:MktRateUnitsByBedrms[3] - 2 Bdrm"     CUAC:MktRateUnitsByBedrms[3] = {  if (IfValidAnd( Proj:NumBedrooms == 2 )) then  SFamMktRateUnitCount  else  0  endif  }
   "Set CUAC:MktRateUnitsByBedrms[4] - 3 Bdrm"     CUAC:MktRateUnitsByBedrms[4] = {  if (IfValidAnd( Proj:NumBedrooms == 3 )) then  SFamMktRateUnitCount  else  0  endif  }
   "Set CUAC:MktRateUnitsByBedrms[5] - 4 Bdrm"     CUAC:MktRateUnitsByBedrms[5] = {  if (IfValidAnd( Proj:NumBedrooms == 4 )) then  SFamMktRateUnitCount  else  0  endif  }
   "Set CUAC:MktRateUnitsByBedrms[6] - 5 Bdrm"     CUAC:MktRateUnitsByBedrms[6] = {  if (IfValidAnd( Proj:NumBedrooms == 5 )) then  SFamMktRateUnitCount  else  0  endif  }
   "Set CUAC:MktRateUnitsByBedrms[7] - 6 Bdrm"     CUAC:MktRateUnitsByBedrms[7] = {  if (IfValidAnd( Proj:NumBedrooms == 6 )) then  SFamMktRateUnitCount  else  0  endif  }
   "Set CUAC:MktRateUnitsByBedrms[8] - >6 Bdrm"    CUAC:MktRateUnitsByBedrms[8] = {  if (IfValidAnd( Proj:NumBedrooms >= 7 )) then  SFamMktRateUnitCount  else  0  endif  }
   "Set CUAC:NumMktRateDwellingUnits - 0-6 Bdrm"   CUAC:NumMktRateDwellingUnits =         ; SAC 09/13/22 (CUAC)
      {  MktRateUnitsByBedrms[1] + MktRateUnitsByBedrms[2] + MktRateUnitsByBedrms[3] + MktRateUnitsByBedrms[4] +
         MktRateUnitsByBedrms[5] + MktRateUnitsByBedrms[6] + MktRateUnitsByBedrms[7]  }

   "Set Proj:NumDwellingUnitsByBedrms[1] - Studio"     Proj:NumDwellingUnitsByBedrms[1] = {  CUAC:AffordableUnitsByBedrms[1] + CUAC:MktRateUnitsByBedrms[1]  }   ; + SumAcrossIf( OldCUACApt:NumMarketRateUnits, OldCUACApt:NumBedrooms = 0 )  }    ; SAC 06/03/24 (tic #1378)  ; SAC 10/22/24
   "Set Proj:NumDwellingUnitsByBedrms[2] - 1 Bdrm"     Proj:NumDwellingUnitsByBedrms[2] = {  CUAC:AffordableUnitsByBedrms[2] + CUAC:MktRateUnitsByBedrms[2]  }   ; + SumAcrossIf( OldCUACApt:NumMarketRateUnits, OldCUACApt:NumBedrooms = 1 )  }
   "Set Proj:NumDwellingUnitsByBedrms[3] - 2 Bdrm"     Proj:NumDwellingUnitsByBedrms[3] = {  CUAC:AffordableUnitsByBedrms[3] + CUAC:MktRateUnitsByBedrms[3]  }   ; + SumAcrossIf( OldCUACApt:NumMarketRateUnits, OldCUACApt:NumBedrooms = 2 )  }
   "Set Proj:NumDwellingUnitsByBedrms[4] - 3 Bdrm"     Proj:NumDwellingUnitsByBedrms[4] = {  CUAC:AffordableUnitsByBedrms[4] + CUAC:MktRateUnitsByBedrms[4]  }   ; + SumAcrossIf( OldCUACApt:NumMarketRateUnits, OldCUACApt:NumBedrooms = 3 )  }
   "Set Proj:NumDwellingUnitsByBedrms[5] - 4 Bdrm"     Proj:NumDwellingUnitsByBedrms[5] = {  CUAC:AffordableUnitsByBedrms[5] + CUAC:MktRateUnitsByBedrms[5]  }   ; + SumAcrossIf( OldCUACApt:NumMarketRateUnits, OldCUACApt:NumBedrooms = 4 )  }
   "Set Proj:NumDwellingUnitsByBedrms[6] - 5 Bdrm"     Proj:NumDwellingUnitsByBedrms[6] = {  CUAC:AffordableUnitsByBedrms[6] + CUAC:MktRateUnitsByBedrms[6]  }   ; + SumAcrossIf( OldCUACApt:NumMarketRateUnits, OldCUACApt:NumBedrooms = 5 )  }
   "Set Proj:NumDwellingUnitsByBedrms[7] - 6 Bdrm"     Proj:NumDwellingUnitsByBedrms[7] = {  CUAC:AffordableUnitsByBedrms[7] + CUAC:MktRateUnitsByBedrms[7]  }   ; + SumAcrossIf( OldCUACApt:NumMarketRateUnits, OldCUACApt:NumBedrooms = 6 )  }
   "Set Proj:NumDwellingUnitsByBedrms[8] - >6 Bdrm"    Proj:NumDwellingUnitsByBedrms[8] = {  CUAC:AffordableUnitsByBedrms[8] + CUAC:MktRateUnitsByBedrms[8]  }   ; + SumAcrossIf( OldCUACApt:NumMarketRateUnits, OldCUACApt:NumBedrooms = 7 )  }
   ;"Set Proj:NumDwellingUnits - 0-6 Bdrm"   Proj:NumDwellingUnits =   
   ;   {  NumDwellingUnitsByBedrms[1] + NumDwellingUnitsByBedrms[2] + NumDwellingUnitsByBedrms[3] + NumDwellingUnitsByBedrms[4] +
   ;      NumDwellingUnitsByBedrms[5] + NumDwellingUnitsByBedrms[6] + NumDwellingUnitsByBedrms[7]  }
   "Set Proj:NumDwellingUnits - 0-6 Bdrm"   Proj:NumDwellingUnits = {  1  }   ; replaced above w/ this to prevent impact on DHW and other analysis results - SAC 10/22/24

   "Set CUAC:ResidentsByBedrms[1] - Studio"              CUAC:ResidentsByBedrms[1]   = {                   min( 1, AffordableUnitsByBedrms[1]  )  }    ; 1 resident per studio unit - SAC 11/21/22 (CUAC)  - SAC 10/22/24
   "Set CUAC:ResidentsByBedrms[2-7]"      for:#U=(2to7)  CUAC:ResidentsByBedrms[#U]  = {  ((#U-1) * 1.5) * min( 1, AffordableUnitsByBedrms[#U] )  }    ; 1.5 residents per bedrm - SAC 11/21/22 (CUAC)
   "Set CUAC:NumResidents - 0-6 Bdrm"                    CUAC:NumResidents = 
      {  ResidentsByBedrms[1] + ResidentsByBedrms[2] + ResidentsByBedrms[3] + ResidentsByBedrms[4] +
         ResidentsByBedrms[5] + ResidentsByBedrms[6] + ResidentsByBedrms[7]  }
   "Set CUAC:ResidentFracByBedrms[1-7]"   for:#U=(1to7)  CUAC:ResidentFracByBedrms[#U]  = {  ResidentsByBedrms[#U] / max( NumResidents, 1 )  } 

   "Set CUAC:AvgUnitAreaByBedrms[1-8]"   for:#U=(1to8)   CUAC:AvgUnitAreaByBedrms[#U] =      ; SAC 12/08/22 (CUAC)
      {  if (IfValidAnd( AffordableUnitsByBedrms[#U] > 0 )==0) then  0
         else  ValidOr( Proj:CondFloorArea, 0 )  endif  } 
   ;      else  SumAcrossIf( ResZn:CondFlrAreaWithMult, ResZn:AffordableUnitsByBedrmsWithMult[#U] > 0 ) / AffordableUnitsByBedrms[#U]  endif  } 

   "Set CUAC:TotAffordableUnitArea"    CUAC:TotAffordableUnitArea =     ; SAC 12/08/22 (CUAC)  - SAC 10/22/24
      {  (AvgUnitAreaByBedrms[1] * min( 1, AffordableUnitsByBedrms[1] )) + (AvgUnitAreaByBedrms[2] * min( 1, AffordableUnitsByBedrms[2] )) +
         (AvgUnitAreaByBedrms[3] * min( 1, AffordableUnitsByBedrms[3] )) + (AvgUnitAreaByBedrms[4] * min( 1, AffordableUnitsByBedrms[4] )) +
         (AvgUnitAreaByBedrms[5] * min( 1, AffordableUnitsByBedrms[5] )) + (AvgUnitAreaByBedrms[6] * min( 1, AffordableUnitsByBedrms[6] )) +
         (AvgUnitAreaByBedrms[7] * min( 1, AffordableUnitsByBedrms[7] )) + (AvgUnitAreaByBedrms[8] * min( 1, AffordableUnitsByBedrms[8] ))  }

   "Set CUAC:PctIndivUnitAreaByBedrms[1-8]"   for:#U=(1to8)   CUAC:PctIndivUnitAreaByBedrms[#U] =     ; SAC 12/08/22 (CUAC)
      {  if (IfValidAnd( TotAffordableUnitArea > 0 )==0) then  0
         else  AvgUnitAreaByBedrms[#U] * 100 / TotAffordableUnitArea  endif  } 

; don't default PctIndivUnitPVByBedrms[*] ??
;   "Default CUAC:PctIndivUnitPVByBedrms[1-8]"   for:#U=(1to8)   CUAC:PctIndivUnitPVByBedrms[#U] = {  PctIndivUnitAreaByBedrms[#U]  }

   "Init CUAC:TotUserDefinedTenantPV"                    CUAC:TotUserDefinedTenantPV = {  0  }
