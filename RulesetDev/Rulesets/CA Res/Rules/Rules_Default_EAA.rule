RULELIST "ForceDefaults_Zone_Status" 1 0 0 0
;*************************************************************************
	"Force Default of Zone IsNew"   			Zone:IsNew			= { if (Status == 3) then 1 else 0 endif }
	"Force Default of Zone IsAltered"   	Zone:IsAltered		= { if (Status == 2) then 1 else 0 endif }
	"Force Default of Zone IsExisting"   	Zone:IsExisting	= { if (Status == 1) then 1 else 0 endif }
END
; SAC 9/10/19 - revisions to all surface ParentIsNewZone rules to account for OtherZone parents (MFamProto)
;*************************************************************************
RULELIST "ForceDefaults_ExtWall_Status" 1 0 0 0
;*************************************************************************
	"Force Default of ExtWall IsNew"   			ExtWall:IsNew			= { if (Status == 3) then 1 else 0 endif }
	"Force Default of ExtWall IsAltered"   	ExtWall:IsAltered		= { if (Status == 2) then 1 else 0 endif }
	"Force Default of ExtWall IsExisting"   	ExtWall:IsExisting	= { if (Status == 1) then 1 else 0 endif }
	"Force consistent ExtWall IsVerified"		ExtWall:IsVerified	= { if (IsNew > 0.5) then 0 else UNCHANGED endif }
   "Set ExtWall:ParentIsNewZone"    ExtWall:ParentIsNewZone  = {
      if (ParentComponentType() != "Zone" .AND. ParentComponentType() != "OtherZone") then  0
      else if (Parent( Status ) == 3) then  1  else  0  endif endif  }
   "Set ExtWall:ParentNotNewZone"   ExtWall:ParentNotNewZone  = {
      if (ParentIsNewZone > 0) then  0  else  1  endif  }

;"dbg" ExtWall:Action = { if (Name != "Extended Front") then  UNCHANGED
;   else  MessageBox( "Cons:Type '%s' / CavityThickness %g / CavityLayerRVal %g", EnumString( Construction:Type ), Construction:CavityThickness, Construction:CavityLayerRVal )  endif  }
   "Set ExtWall:ShowException - flag indicating whether to display Exception choices"      ; SAC 5/18/18 - tic #798
      ExtWall:ShowException  = {
         if (ParentComponentType() != "Zone" .OR. LocalCompAssigned( Construction )==0) then  0
         else if (Zone:Status != 3 .OR. Status != 3) then  0
         else if (Construction:Type != 0) then  0   ; not wood-framed wall
         else if ((((int( Construction:FrameLayer / 100 ) - 0.5) == 3.5 .AND. Construction:CavityLayerRVal >= 15) .OR.
                   ((int( Construction:FrameLayer / 100 ) - 0.5) == 5.5 .AND. Construction:CavityLayerRVal >= 18)) .AND.   ; R18 = R19 in 5.5in cavity
                  ( Proj:RunScope == 2 .OR.   ; EAA
                   (Proj:RunScope == 1 .AND. Proj:IsAddAlone > 0))) then  1
         else  0  endif endif endif endif  }
   "Backward compatibility for ExtWall:IsWoodFrmdWallExt -> Exception"      ; SAC 5/18/18 - tic #798
      ExtWall:Action  = {
         if (IfValidAnd( IsWoodFrmdWallExt > 0 ) .AND. ShowException > 0 .AND.
             LocalStatus( IsWoodFrmdWallExt ) > 6 .AND. LocalStatus( Exception ) < 7)
         then  EvalRulelist( "Force_ExtWall_Exception_WallExt", 1 )
         else  UNCHANGED  endif  }
   "Blast any user-defined ExtWall:IsWoodFrmdWallExt (no longer an input)"     ; SAC 5/18/18 - tic #798
      ExtWall:Action  = {
         if (LocalStatus( IsWoodFrmdWallExt ) > 6)
         then  EvalRulelist( "ForceReset_ExtWall_IsWoodFrmdWallExt", 1 )
         else  UNCHANGED  endif  }
   "Reset ExtWall:Exception if not a valid input for this ExtWall"     ; SAC 5/18/18 - tic #798
      ExtWall:Action  = {
         if (Exception > 0 .AND. ShowException == 0)
         then  EvalRulelist( "ForceReset_ExtWall_Exception", 1 )
         else  UNCHANGED  endif  }
;   "Default ExtWall:IsWoodFrmdWallExt - whether or not this is a wood-framed wall extension of an existing wall (Section 150.2(a)1A)"  ; SAC 4/30/18 - tic #798
;      ExtWall:IsWoodFrmdWallExt  = {  0  }
;  allow default via Enums ->  "Exception",  BEMP_Sym,  "exterior wall exception: none, Wall Extension or Existing Wall w/ Siding"  ; SAC 5/18/18 - replacement for IsWoodFrmdWallExt (tic #798)
   "Set ExtWall:ExceptionRpt"    ExtWall:ExceptionRpt =     ; SAC 5/19/18 - tic #798
      {  if (ShowException < 1) then  "n/a"
         else  switch (Exception)
                  case  1 :  "Extension"
                  case  2 :  "Ex. w/ Siding"
                  default :  "none"
               endswitch  endif  }

   ; SAC 5/18/18 - revised QII rules to accommodate wall exception input (tic #798)
   "Set ExtWall:QIIDegraded"     ExtWall:QIIDegraded  = {
      if (LocalCompAssigned( Construction ) > 0 .AND.
          (IfValidAnd( Construction:Type = 0 ) .OR.  ; wood framed
           IfValidAnd( Construction:Type = 1 ) .OR.  ; steel framed
           IfValidAnd( Construction:Type = 5 ) .OR.  ; SIPS wall
           IfValidAnd( Construction:Type = 6 )))     ; DPHwall
      then  if (ParentNotNewZone > 0 .OR. Proj:InsulConsQuality < 2 .OR.
                (ShowException > 0 .AND. Exception > 0))
            then  1  else  0  endif
      else  0  endif  }
   "Set ExtWall:QIINotDegraded"  ExtWall:QIINotDegraded  = {
      if (QIIDegraded > 0) then  0  else  1  endif  }
END
