      {  if ((Proj:BypassRuleLimits > 0 .OR. Proj:ApplyStdDesignIAQFromTable) .AND. IfValidAnd( Proj:StdIAQHtRec_SRE[1] > 0 ) .AND.
             IfValidAnd( Proj:StdDesignIAQType[1] = 4 )) 
         then  Proj:StdIAQHtRec_SRE[1]  else  UNCHANGED  endif  }
