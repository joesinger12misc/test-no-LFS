   "Override Proj:UnitIAQFan2[1]:IAQFanType in certain circumstances"      Proj:UnitIAQFan2[1]:IAQFanType  =
      {  if (IsMultiFamily > 0.5 .OR. (IfValidAnd( BypassRuleLimits > 0 )==0 .AND. ApplyStdDesignIAQFromTable < 1)) then  UNCHANGED
         else if (IfValidAnd( StdDesignIAQType[1] > 0 )==0) then  UNCHANGED
         else  min( StdDesignIAQType[1], 3 )  endif endif  }
   "Override Proj:UnitIAQFan3[1]:IAQFanType in certain circumstances"      Proj:UnitIAQFan3[1]:IAQFanType  =
      {  if (IsMultiFamily > 0.5 .OR. (IfValidAnd( BypassRuleLimits > 0 )==0 .AND. ApplyStdDesignIAQFromTable < 1)) then  UNCHANGED
         else if (IfValidAnd( StdDesignIAQType[1] > 0 )==0) then  UNCHANGED
         else  min( StdDesignIAQType[1], 3 )  endif endif  }
   "Override Proj:UnitIAQFan4[1]:IAQFanType in certain circumstances"      Proj:UnitIAQFan4[1]:IAQFanType  =
      {  if (IsMultiFamily > 0.5 .OR. (IfValidAnd( BypassRuleLimits > 0 )==0 .AND. ApplyStdDesignIAQFromTable < 1)) then  UNCHANGED
         else if (IfValidAnd( StdDesignIAQType[1] > 0 )==0) then  UNCHANGED
         else  min( StdDesignIAQType[1], 3 )  endif endif  }

   "Override all MFam IAQFan:IAQFanType (in certain circumstances) - only effective for IAQ via assigned fans"     IAQFan:IAQFanType  =    ; SAC 5/13/20    ; SAC 08/27/20
      {  if (Proj:IsMultiFamily < 0.5 .OR. (IfValidAnd( Proj:BypassRuleLimits > 0 )==0 .AND.
                                            Proj:ApplyStdDesignIAQFromTable < 1)) then  UNCHANGED
         else if (IfValidAnd( Proj:StdDesignIAQType[1] > 0 )==0) then  UNCHANGED
         else if (Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf" .OR. 
                  (IfValidAnd( Proj:ApplyStdIAQMaxPwrToDRtg > 0 ) .AND. IfValidAnd( Proj:BypassRuleLimits > 0 ))) 
         then  min( Proj:StdDesignIAQType[1], 3 )
         else  UNCHANGED  endif endif endif  }
   ; secondary rule to address MFam IAQFan type change for DwellUnitType generic (non-fan) settings - SAC 5/13/20       ; SAC 08/27/20
   "Override MFam DwellUnitType IAQ Type (in certain circumstances) - IAQ w/out assigned fans"   DwellUnitType:IAQOption  =
      {  if (Proj:IsMultiFamily < 0.5 .OR. (IfValidAnd( Proj:BypassRuleLimits > 0 )==0 .AND.
                                            Proj:ApplyStdDesignIAQFromTable < 1)) then  UNCHANGED
         else if (IfValidAnd( Proj:StdDesignIAQType[1] > 0 )==0) then  UNCHANGED
         else if (Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf" .OR.
                  (IfValidAnd( Proj:ApplyStdIAQMaxPwrToDRtg > 0 ) .AND. IfValidAnd( Proj:BypassRuleLimits > 0 ))) 
         then  if (     Proj:StdDesignIAQType[1]  = 1 .AND. IAQOption = 4) then  5     ; Balanced -> Exhaust
               else if (Proj:StdDesignIAQType[1] >= 3 .AND. IAQOption = 5) then  4     ; Exhaust -> Balanced
               else  UNCHANGED  endif endif
         else  UNCHANGED  endif endif endif  }

