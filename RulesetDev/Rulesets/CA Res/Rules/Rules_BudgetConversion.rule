		     ((Proj:IsAddAlone > 0.5 .AND. MinRevRef( Zone:ActiveHVACSystem, Zone:HVACSysStatus ) < 2) .OR. 
            (Proj:RunScope == 2 .AND. MinRevRef( Zone:ActiveHVACSystem, Zone:HVACSysStatus ) < 2)) )) 
    then  UNCHANGED
		else if (T24RCoolingEquipment:DuctFlag( "CodeBase", Proj:StdDesignBase, "StdVer", Proj:StandardsVersion, "SysType", CoolSysTypeAbbrev ) != 0) then 1
		else  UNCHANGED
