	"Set OtherZone:ExtWallAreaByWallType[3] - concrete/CMU - add new Skylt area"	OtherZone:ExtWallAreaByWallType[3]	= {	if (ExtWallAreaByWallType[3] > ExtWallAreaByWallType[1] .AND. ExtWallAreaByWallType[3] > ExtWallAreaByWallType[2])
																																					then  ExtWallAreaByWallType[3] + SumChildren( CathedralCeiling:ChildAreaSum[2] ) + SumChildren( CathedralCeiling:ChildAreaSum[4] )   else  UNCHANGED  endif  }
	"Set OtherZone:ExtWallAreaByWallType[4] - total"				OtherZone:ExtWallAreaByWallType[4]		= {   ExtWallAreaByWallType[1] + ExtWallAreaByWallType[2] + ExtWallAreaByWallType[3]  }

	"Set OtherZone:ExtWallWinAreaByWallType[1] - wood framed"	OtherZone:ExtWallWinAreaByWallType[1]	= {   SumChildren( ExtWall:NewWinAreaByWallType[1] )  }
	"Set OtherZone:ExtWallWinAreaByWallType[2] - steel framed"	OtherZone:ExtWallWinAreaByWallType[2]	= {   SumChildren( ExtWall:NewWinAreaByWallType[2] )  }
	"Set OtherZone:ExtWallWinAreaByWallType[3] - concrete/CMU"	OtherZone:ExtWallWinAreaByWallType[3]	= {   SumChildren( ExtWall:NewWinAreaByWallType[3] )  }
	"Set OtherZone:ExtWallWinAreaByWallType[4] - total"			OtherZone:ExtWallWinAreaByWallType[4] 	= {   ExtWallWinAreaByWallType[1] + ExtWallWinAreaByWallType[2] + ExtWallWinAreaByWallType[3]  }
	"Set OtherZone:ExtWallDoorArea"										OtherZone:ExtWallDoorArea					= {   SumChildren( ExtWall:TotDoorArea )  }
	"Set OtherZone:ExtWallDoorAreaByWallType[1] - wood framed"	OtherZone:ExtWallDoorAreaByWallType[1]	= {   SumChildren( ExtWall:DoorAreaByWallType[1] )  }
	"Set OtherZone:ExtWallDoorAreaByWallType[2] - steel framed"	OtherZone:ExtWallDoorAreaByWallType[2]	= {   SumChildren( ExtWall:DoorAreaByWallType[2] )  }
	"Set OtherZone:ExtWallDoorAreaByWallType[3] - concrete/CMU"	OtherZone:ExtWallDoorAreaByWallType[3]	= {   SumChildren( ExtWall:DoorAreaByWallType[3] )  }
