      {  if (((IfValidAnd( Proj:BypassRuleLimits > 0 ) .AND. IfValidAnd( Proj:AnalysisVersion >= 190200 )) .OR. Proj:ApplyStdDesignIAQFromTable) .AND.
             IfValidAnd( Proj:StdDesignIAQType[1] = 4 ) .AND.
             IfValidAnd( Proj:StdIAQHtRec_SRE[1] > 0 ) .AND. IfValidAnd( Proj:StdIAQHtRec_ASRE[1] > 0 ))
         then  UNCHANGED  ; should already be toggled on
         else  0  endif  }
      ;   else if (IfValidAnd( Proj:AnalysisVersion >= 190200 ) .AND. Proj:IsMultiFamily == 0 .AND.
      ;            (Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf"))
      ;   then  UNCHANGED
