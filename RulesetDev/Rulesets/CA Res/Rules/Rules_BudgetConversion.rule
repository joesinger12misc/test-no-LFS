;*************************************************************************  - SAC 1/9/18
RULELIST "Setup_StdDesignPV"  1 0 1 1
	"Determine which RunResults is Worst case (based on PrelimPropCompTDV for 2019+)"	Proj:WorstOrientation	= {
         if (AllOrientations < 1) then  UNCHANGED
         else if (ResultSummary[2]:PrelimPropCompTDV >= ResultSummary[3]:PrelimPropCompTDV .AND.
						ResultSummary[2]:PrelimPropCompTDV >= ResultSummary[4]:PrelimPropCompTDV .AND.
						ResultSummary[2]:PrelimPropCompTDV >= ResultSummary[5]:PrelimPropCompTDV )	then	1
			else if (ResultSummary[3]:PrelimPropCompTDV >= ResultSummary[2]:PrelimPropCompTDV .AND.
						ResultSummary[3]:PrelimPropCompTDV >= ResultSummary[4]:PrelimPropCompTDV .AND.
						ResultSummary[3]:PrelimPropCompTDV >= ResultSummary[5]:PrelimPropCompTDV )	then	2
			else if (ResultSummary[4]:PrelimPropCompTDV >= ResultSummary[2]:PrelimPropCompTDV .AND.
						ResultSummary[4]:PrelimPropCompTDV >= ResultSummary[3]:PrelimPropCompTDV .AND.
						ResultSummary[4]:PrelimPropCompTDV >= ResultSummary[5]:PrelimPropCompTDV )	then	3	else	4	endif endif endif endif  }
;"dbg" Proj:Action = { if (AllOrientations > 0) then  PostMessageToLog( "   just set Proj:WorstOrientation to %g", WorstOrientation )  else  UNCHANGED  endif  }

   ; toggle off Battery and setup PV system based on StdMixedFuelPVMlt
   "Zero-out Battery (Proj:BattMaxCap)"   Proj:BattMaxCap  = {  0  }
   "Set Proj:PVWInputs to Simplified"     Proj:PVWInputs   = {  0  }   ; "Simplified"
   "Set Proj:PVWDCSysSize[1] to 1.0"      Proj:PVWDCSysSize[1]   = {
         ValidOr( ResultSummary[1]:StdMixedFuelPVMlt, 0 )  }
;"dbg" Proj:Action = { PostMessageToLog( "   just set Std design Proj:PVWDCSysSize[1] = %g", PVWDCSysSize[1] ) }

   ; SAC 1/17/18 - revise logic to ensure use of UseReducedPVReqVal when ResultSummary[1]:StdMixedFuelPVMlt not valid
   "Adjust PVWDCSysSize[1] for Reduced PV Requirement (ReducedPVReq)"   Proj:PVWDCSysSize[1] = {   ; SAC 1/11/18
         if (IfValidAnd( UseReducedPVReqVal >= 0 ) .AND.
             (IfValidAnd( PVWDCSysSize[1] > 0 )==0 .OR.
              IfValidAnd( PVWDCSysSize[1] > UseReducedPVReqVal )))
         then  UseReducedPVReqVal
         else  UNCHANGED  endif  }
;"dbg" Proj:Action = { if (IfValidAnd( PVWDCSysSize[1] = UseReducedPVReqVal )) then  PostMessageToLog( "   adjusted Proj:PVWDCSysSize[1] to %g (due to ReducedPVReq)", PVWDCSysSize[1] )  else  UNCHANGED  endif }

   "Set Proj:PVWDCSizeMultiplier[1] to 1" Proj:PVWDCSizeMultiplier[1] = { 1 }
   "Set Proj:PVWModuleType[1] to Std"     Proj:PVWModuleType[1]  = {  1  }   ; "Standard"
   "Set Proj:PVWArrayType[1] to Fixed"    Proj:PVWArrayType[1]   = {  1  }   ; "Fixed (open rack)"    ; SAC 10/10/18 (tic #1047)
   "Set Proj:PVWPwrElec[1] to None"       Proj:PVWPwrElec[1]     = {  0  }   ; "- none -"    ; SAC 1/15/19 (tic #1046)
   "default PVWInverterEff[1] to 96"      Proj:PVWInverterEff[1] = {  96  }   ; SAC 6/10/20
   "Toggle on CFI (Proj:PVWCalFlexInstType[1])" Proj:PVWCalFlexInstType[1] = {  1  }   ; SAC 1/20/20 (tic #1189)
   "Toggle on CFI (Proj:PVWCalFlexInstall[1])"  Proj:PVWCalFlexInstall[1]  = {  1  }
   "Blast Proj:PVWCoverRefrInd[1]"        Proj:PVWCoverRefrInd[1]  = {  UNDEFINED  }      ; SAC 4/20/20
   "Blast Proj:PVWTempCoeff[1]"           Proj:PVWTempCoeff[1]     = {  UNDEFINED  }      ; SAC 4/20/20
   "Blast Proj:PVWDCSysSize[2]"           Proj:PVWDCSysSize[2]   = {  UNDEFINED  }
   "Blast Proj:PVWDCSysSize[3]"           Proj:PVWDCSysSize[3]   = {  UNDEFINED  }
   "Blast Proj:PVWDCSysSize[4]"           Proj:PVWDCSysSize[4]   = {  UNDEFINED  }
   "Blast Proj:PVWDCSysSize[5]"           Proj:PVWDCSysSize[5]   = {  UNDEFINED  }
END
