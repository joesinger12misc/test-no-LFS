      {  if (IfValidAnd( DwellUnitTypeRef:IAQDisabled > 0 ) .OR.
             (IfValidAnd( Proj:BypassRuleLimits > 0 )==0 .AND. Proj:ApplyStdDesignIAQFromTable < 1) .OR.
             (GlobalStatus( Proj:StdDesignIAQWperCFM ) < 4 .AND. LocalStatus( DwellUnitTypeRef:MaxStdDesignIAQWperCFM ) < 4) .OR.
             (Proj:RunAbbrev != "s" .AND. Proj:RunAbbrev != "smf" .AND.
              (Proj:RunAbbrev != "dr" .OR. (IfValidAnd( Proj:ApplyStdIAQMaxPwrToDRtg > 0 )==0 .AND. IfValidAnd( Proj:BypassRuleLimits > 0 ))))) then  UNCHANGED
