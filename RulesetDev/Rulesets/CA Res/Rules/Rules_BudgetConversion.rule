                     (IfValidAnd( Proj:BypassRuleLimits > 0 ) .AND. (Proj:ApplyStdIAQMaxPwrToDRtg .OR. Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf")) ))
		         then  Proj:StdDesignIAQWperCFM
               else if (IAQFanType == 3)  ; Balanced
		         then  0.7  else  0.35  endif endif
		   else if (IfValidAnd( Proj:AnalysisVersion <= 190101 ))
         then  switch ( IAQFanType )
                  case  1 :   Min( WperCFMIAQ, 1.2 )      ; "Exhaust"                                      
                  case  2 :   Min( WperCFMIAQ, 1.2 )      ; "Supply"                                       
                  case  3 :   Min( WperCFMIAQ, 1.2 )      ; "Balanced"                                     
               ;   case  4 :   Min( WperCFMIAQ, 0.58 )      ; "Central"  - not yet implemented - MJB 4/22/13 
                  default :   UNDEFINED
               endswitch
         else if (Proj:IsMultiFamily > 0) then  UNCHANGED
         else if (Proj:ApplyStdIAQMaxPwrToDRtg .OR. Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf")
         then  switch ( IAQFanType )
                  case  1 :   Min( WperCFMIAQ, Proj:StdDesignIAQWperCFM )      ; "Exhaust"                                      
                  case  2 :   Min( WperCFMIAQ, Proj:StdDesignIAQWperCFM )      ; "Supply"                                       
                  case  3 :   Min( WperCFMIAQ, Proj:StdDesignIAQWperCFM )      ; "Balanced"                                     
               ;   case  4 :   Min( WperCFMIAQ, 0.58 )      ; "Central"  - not yet implemented - MJB 4/22/13 
                  default :   UNDEFINED
               endswitch
         else  UNCHANGED  endif endif endif endif  }

   ; SAC 1/11/16 - ensure IAQRecovEffect is zero (tic 599)
   "Reset IAQFan:IAQRecovEffect"	IAQFan:IAQRecovEffect = {  0  }     ; No Heat Recovery
   ; SAC 2/5/20 (tic #1174)      ; modified to RETAIN HtRecov in Std & SMF models for SFam in versions 19.2.0+ - SAC 7/20/20 (tic #1174)
   ; revised for more project-level testing inputs - SAC 08/27/20
