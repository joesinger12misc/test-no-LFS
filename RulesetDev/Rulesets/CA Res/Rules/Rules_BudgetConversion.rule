;*************************************************************************  - rules to PREVENT any Proposed model IAQ credit (results in std model higher or equal eff to prop) - SAC 01/26/21 (tic #1243)
RULELIST  "BudgetConversion_IAQ_SupplyInletsNotAccessible" 1 0 1 0

   "Set IAQFan:HardwireStdFanPower flag"     IAQFan:HardwireStdFanPower =     ; SAC 03/18/21 (tic #1243)
      {  if ( (Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf") .AND.
              ( (IAQFanType == 3 .AND. (ValidOr( WperCFMIAQ, 0 ) * ValidOr( FanPowerMult, 1 )) > 0.7 ) .OR.  ; Balanced
                (IAQFanType != 3 .AND. (ValidOr( WperCFMIAQ, 0 ) * ValidOr( FanPowerMult, 1 )) > 0.35) ) )   ; Exhaust or Supply
         then  1.0
         else  UNDEFINED  endif  }
   "Hardwire IAQFan:FanPowerMult to 1.0 if Std/SMF power lower than Prop"     IAQFan:FanPowerMult =      ; SAC 03/18/21 (tic #1243)
      {  if (IfValidAnd( HardwireStdFanPower > 0 ))
         then  1.0
         else  UNCHANGED  endif  }  
	"Ensure Std Watts per CFM <= Proposed"    IAQFan:WperCFMIAQ =     ; revised to reference HardwireStdFanPower - SAC 03/18/21 (tic #1243)
		{  if (IfValidAnd( HardwireStdFanPower > 0 ))
         then  if (IAQFanType == 3)        ; Balanced
               then  0.7
               else  0.35  endif
         else  UNCHANGED  endif  }

;   "Reset IAQFan:IAQRecovEffect"	IAQFan:IAQRecovEffect = {  0  }      - RETAIN heat recovery settings
;   "Reset IAQFan:IncludesRecov"  IAQFan:IncludesRecov  =      
;   "Set IAQFan:FanPowerMult"        IAQFan:FanPowerMult     = {  1.0  }  
;   "Set IAQFan:FanHeatRecovMult"    IAQFan:FanHeatRecovMult = {  1.0  }
;   "Set IAQFan:SensRecovEff"        IAQFan:SensRecovEff =    
;   "Set IAQFan:AdjSensRecovEff"     IAQFan:AdjSensRecovEff =         ; SAC 08/29/20

   "Set Zone:IAQCFMScaleFactor[1] if appropriate"     Zone:IAQCFMScaleFactor[1]  =        ; SAC 9/9/19 - new logic to prevent MFam evaluation
      {  if (Proj:IsMultiFamily == 0 .AND. IfValidAnd( Proj:IAQCFMScaleFactor[1] > 0 ) .AND.
             ( (Proj:ApplyStdIAQCFMLimitToDRtg .AND. IfValidAnd( Proj:AnalysisVersion > 190101 )) .OR.
               Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf" )) then  Proj:IAQCFMScaleFactor[1]
         else if (Proj:IsMultiFamily > 0 .AND. IfValidAnd( StdDesignIAQCFM > 0 ) .AND.
                  (IAQCFM_Exhaust[1] + IAQCFM_Supply[1] + IAQHRVCFM[1]) > 0.1 .AND.
                  ( (Proj:ApplyStdIAQCFMLimitToDRtg .AND. IfValidAnd( Proj:AnalysisVersion > 190101 )) .OR.
                    Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf" ))
         then  StdDesignIAQCFM / (IAQCFM_Exhaust[1] + IAQCFM_Supply[1] + IAQHRVCFM[1])
         else  UNDEFINED  endif endif  }
   "Set Zone:IAQCFMScaleFactor[2] if appropriate"     Zone:IAQCFMScaleFactor[2]  =        ; SAC 9/9/19 - new logic to prevent MFam evaluation
      {  if (Proj:IsMultiFamily == 0 .AND. IfValidAnd( Proj:IAQCFMScaleFactor[2] > 0 ) .AND.
             ( (Proj:ApplyStdIAQCFMLimitToDRtg .AND. IfValidAnd( Proj:AnalysisVersion > 190101 )) .OR.
               Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf" )) then  Proj:IAQCFMScaleFactor[2]
         else  UNDEFINED  endif  }
   "Set Zone:IAQPwrScaleFactor[1] if appropriate"     Zone:IAQPwrScaleFactor[1]  =        ; SAC 12/16/19 - added separate scaling factor for IAQ power (for MFam)   ; remove for AnalysisVersion >= 190200 - SAC 01/26/21 (tic #1243)
      {  if (IfValidAnd( Proj:AnalysisVersion < 190200 ) .AND.
             Proj:IsMultiFamily > 0 .AND. IfValidAnd( MaxStdDesignIAQPower > 0 ) .AND.
             (IAQPwr_Exhaust[1] + IAQPwr_Supply[1] + IAQHRVPwr[1]) > 0.1 .AND.
             ( (Proj:ApplyStdIAQMaxPwrToDRtg .AND. IfValidAnd( Proj:AnalysisVersion > 190101 )) .OR.
               Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf" ))
         then  min( 1, MaxStdDesignIAQPower / (IAQPwr_Exhaust[1] + IAQPwr_Supply[1] + IAQHRVPwr[1]) )
         else  UNDEFINED  endif  }

   "Set OtherZone:IAQCFMScaleFactor if appropriate"      OtherZone:IAQCFMScaleFactor  =      ; SAC 12/15/19
      {  if (Proj:IsMultiFamily == 0 .AND. IfValidAnd( Proj:IAQCFMScaleFactor[1] > 0 ) .AND.
             ( (Proj:ApplyStdIAQCFMLimitToDRtg .AND. IfValidAnd( Proj:AnalysisVersion > 190101 )) .OR.
               Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf" )) then  Proj:IAQCFMScaleFactor[1]
         else if (Proj:IsMultiFamily > 0 .AND. IfValidAnd( StdDesignIAQCFM > 0 ) .AND.
                  (IAQCFM_Exhaust[1] + IAQCFM_Supply[1] + IAQHRVCFM[1]) > 0.1 .AND.
                  ( (Proj:ApplyStdIAQCFMLimitToDRtg .AND. IfValidAnd( Proj:AnalysisVersion > 190101 )) .OR.
                    Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf" ))
         then  StdDesignIAQCFM / (IAQCFM_Exhaust[1] + IAQCFM_Supply[1] + IAQHRVCFM[1])
         else  UNDEFINED  endif endif  }
   "Set OtherZone:IAQPwrScaleFactor if appropriate"      OtherZone:IAQPwrScaleFactor  =      ; SAC 12/16/19 - added separate scaling factor for IAQ power (for MFam)   ; remove for AnalysisVersion >= 190200 - SAC 01/26/21 (tic #1243)
      {  if (IfValidAnd( Proj:AnalysisVersion < 190200 ) .AND.
             Proj:IsMultiFamily > 0 .AND. IfValidAnd( MaxStdDesignIAQPower > 0 ) .AND.
             (IAQPwr_Exhaust[1] + IAQPwr_Supply[1] + IAQHRVPwr[1]) > 0.1 .AND.
             ( (Proj:ApplyStdIAQMaxPwrToDRtg .AND. IfValidAnd( Proj:AnalysisVersion > 190101 )) .OR.
               Proj:RunAbbrev == "s" .OR. Proj:RunAbbrev == "smf" ))
         then  min( 1, MaxStdDesignIAQPower / (IAQPwr_Exhaust[1] + IAQPwr_Supply[1] + IAQHRVPwr[1]) )
         else  UNDEFINED  endif  }

END
