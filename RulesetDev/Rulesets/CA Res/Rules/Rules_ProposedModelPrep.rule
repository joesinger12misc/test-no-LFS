;*************************************************************************  - PV system scaling (tic #923) - SAC 1/2/18
RULELIST  "ApplyPVSystemScaling" 1 0 0 1
;*************************************************************************
;"log status"   Proj:Action = {  if (DevMode==0 .OR. LocalStatus(RunAbbrev) < 1 .OR. EnergyCodeYearNum < 2019) then  UNCHANGED  else
;   PostMessageToLog( "    evaluating 'ApplyPVSystemScaling' rules on '%s' model, PVSizeOption %g, ...", RunAbbrev, PVSizeOption )  endif  }
   ;      if (RunAbbrev != "p")
   ;      then  PostMessageToLog( "   PV scaling of multiple orientation models not yet implemented" )
;"store BEMProc details"	 Proj:Action = {  StoreBEMProc( Format( "b4 PV scaling-%s", RunAbbrev ), 0 )  }
;"debug" Proj:Action = { MessageBox( "Just stored BEM details prior to PV scaling for run '%s'.", RunAbbrev )  }
;"debug" Proj:Action = { MessageBox( "Inside ApplyPVSystemScaling rulelist for run '%s':\PVSizeOption %g  ResultSummary[1]:StdMixedFuelPVMlt %g", RunAbbrev, PVSizeOption, ResultSummary[1]:StdMixedFuelPVMlt )  }
   "Identify issues in scaling PV or initiate scaling"   Proj:Action  = {
         if (PVSizeOption == 1)       ; Standard Design PV
         then       if (RunAbbrev = "p"   .AND. (IfValidAnd( ResultSummary[1]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesign" )
               else if (RunAbbrev = "p-N" .AND. (IfValidAnd( ResultSummary[2]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesign_N" )
               else if (RunAbbrev = "p-E" .AND. (IfValidAnd( ResultSummary[3]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesign_E" )
               else if (RunAbbrev = "p-S" .AND. (IfValidAnd( ResultSummary[4]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesign_S" )
               else if (RunAbbrev = "p-W" .AND. (IfValidAnd( ResultSummary[5]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesign_W" )
               else  PostMessageToLog( "   Unable to scale standard PV system - bad or missing EUseSummary:StdMixedFuelPVMlt and/or Proj:PVWDCSysTotal" )
               endif endif endif endif endif
         else if (PVSizeOption == 2)       ; Standard Design PV TDV        ; SAC 6/9/20 (CommunitySolar)
         then       if (RunAbbrev = "p"   .AND. (IfValidAnd( ResultSummary[1]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesignTDV" )
               else if (RunAbbrev = "p-N" .AND. (IfValidAnd( ResultSummary[2]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesignTDV_N" )
               else if (RunAbbrev = "p-E" .AND. (IfValidAnd( ResultSummary[3]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesignTDV_E" )
               else if (RunAbbrev = "p-S" .AND. (IfValidAnd( ResultSummary[4]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesignTDV_S" )
               else if (RunAbbrev = "p-W" .AND. (IfValidAnd( ResultSummary[5]:StdMixedFuelPVMlt > 0 ) .OR. IfValidAnd( UseReducedPVReqVal >= 0 )))
               then  EvalRulelist( "ApplyPVSystemScaling_StandardDesignTDV_W" )
               else  PostMessageToLog( "   Unable to scale standard PV (TDV) system - bad or missing EUseSummary:StdMixedFuelPVMlt and/or Proj:PVWDCSysTotal" )
               endif endif endif endif endif
         else if (PVSizeOption == 3)  ; Maximum PV for Compliance Credit
         then       if (RunAbbrev = "p"   .AND. IfValidAnd( ResultSummary[1]:PropInputPVMult > 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_MaxCompliance" )
               else if (RunAbbrev = "p-N" .AND. IfValidAnd( ResultSummary[2]:PropInputPVMult > 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_MaxCompliance_N" )
               else if (RunAbbrev = "p-E" .AND. IfValidAnd( ResultSummary[3]:PropInputPVMult > 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_MaxCompliance_E" )
               else if (RunAbbrev = "p-S" .AND. IfValidAnd( ResultSummary[4]:PropInputPVMult > 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_MaxCompliance_S" )
               else if (RunAbbrev = "p-W" .AND. IfValidAnd( ResultSummary[5]:PropInputPVMult > 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_MaxCompliance_W" )
               else  PostMessageToLog( "   Unable to scale proposed PV system - bad or missing EUseSummary:PropInputPVMult" )
               endif endif endif endif endif
         else if (PVSizeOption == 10)  ; Specify PV System Scaling
         then  if (IfValidAnd( PVSizeMultiplier > 0 )==0)
               then  PostMessageToLog( "   Unable to scale proposed PV system - bad or missing Proj:PVSizeMultiplier" )
               else if (IfValidAnd( PVSizeMultiplier <= PVSizeMultMax )==0)
               then  PostMessageToLog( "   Unable to scale proposed PV system - Proj:PVSizeMultiplier of %g exceeds compliance maximum of %g", PVSizeMultiplier, PVSizeMultMax )
               else if (RunAbbrev = "p"   .AND. IfValidAnd( ResultSummary[1]:PrelimPropkWh > 0 )
                                          .AND. IfValidAnd( ResultSummary[1]:PrelimPropPV  < 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_SpecifyScaling" )
               else if (RunAbbrev = "p-N" .AND. IfValidAnd( ResultSummary[2]:PrelimPropkWh > 0 )
                                          .AND. IfValidAnd( ResultSummary[2]:PrelimPropPV  < 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_SpecifyScaling_N" )
               else if (RunAbbrev = "p-E" .AND. IfValidAnd( ResultSummary[3]:PrelimPropkWh > 0 )
                                          .AND. IfValidAnd( ResultSummary[3]:PrelimPropPV  < 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_SpecifyScaling_E" )
               else if (RunAbbrev = "p-S" .AND. IfValidAnd( ResultSummary[4]:PrelimPropkWh > 0 )
                                          .AND. IfValidAnd( ResultSummary[4]:PrelimPropPV  < 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_SpecifyScaling_S" )
               else if (RunAbbrev = "p-W" .AND. IfValidAnd( ResultSummary[5]:PrelimPropkWh > 0 )
                                          .AND. IfValidAnd( ResultSummary[5]:PrelimPropPV  < 0 ))
               then  EvalRulelist( "ApplyPVSystemScaling_SpecifyScaling_W" )
               else  PostMessageToLog( "   Unable to scale proposed PV system - bad or missing EUseSummary:PrelimPropkWh and/or EUseSummary:PrelimPropPV" )
               endif endif endif endif endif endif endif
         else  PostMessageToLog( "   Unable to scale proposed PV system - unrecognized PVSizeOption" )
         endif endif endif endif  }

;"log status"   Proj:Action = {  if (DevMode==0 .OR. LocalStatus(RunAbbrev) < 1 .OR. EnergyCodeYearNum < 2019 .OR. AllOrientations < 1) then  UNCHANGED  else
;     PostMessageToLog( "    following 'ApplyPVSystemScaling' evals, PropInputPVMult for N %.3f, E %.3f, S %.3f, W %.3f", ResultSummary[2]:PropInputPVMult, ResultSummary[3]:PropInputPVMult, ResultSummary[4]:PropInputPVMult, ResultSummary[5]:PropInputPVMult )  endif  }
END
;*************************************************************************  - PV system scaling (tic #923) - SAC 1/2/18
RULELIST  "ApplyPVSystemScaling_StandardDesign" 1 0 0 1
   "Set Proj:PVWDCSizeMultiplier[1] to achieve 'Standard Design PV' scaling"
      Proj:PVWDCSizeMultiplier[1]  = {
         if (IfValidAnd( ResultSummary[1]:StdMixedFuelPVMlt > 0 ))
         then  ResultSummary[1]:StdMixedFuelPVMlt / PVWDCSysTotal
         else if (IfValidAnd( UseReducedPVReqVal >= 0 ))
         then  UseReducedPVReqVal / PVWDCSysTotal
         else  UNCHANGED  endif endif  }
   "Set EUseSummary:PropInputPVMult"
      Proj:ResultSummary[1]:PropInputPVMult  = {  Proj:PVWDCSizeMultiplier[1]  }
   "Set ResultSummary[1]:UserPVScalingMsg to document PV scaling"
      Proj:ResultSummary[1]:UserPVScalingMsg  = {
         if (IfValidAnd( UseReducedPVReqVal < 0.0001 ))
         then          "PV System(s) removed due to Reduced PV Requirement of 0 kWdc"
         else if (IfValidAnd( Proj:PVWLastRow > 1 ))
