; SAC 5/14/18 - rulelist to handle Existing skylts in Altered CathedralCeilings (tic #843)
;*************************************************************************
RULELIST "SetupExistingSkyltCathedralCeilings" 1 0 0 1
; before getting here we have checked that this is a EAA or AA model w/ 1 or more Altered CathedralCeilings and 1 or more Existing Skylts

   "Set CathedralCeiling:AltClgExSkyltArea"     CathedralCeiling:AltClgExSkyltArea  = {
         if (Status != 2) then  0
         else  SumChildrenIf( Skylt:TotAreaInclMult, Skylt:Status = 1 )  endif  }
   "Create CathedralCeiling:ExSkyltCathClgRef when needed to ensure Existing Skylt(s) persist in budget design"
      CathedralCeiling:ExSkyltCathClgRef  = {
         if (AltClgExSkyltArea < 0.1) then  UNDEFINED
         else  CreateComp( CathedralCeiling, Name + "-ExSkylt" )  endif  }
   ; setup new CathedralCeiling
   "Set ExSkyltCathClgRef:Parent"      CathedralCeiling:ExSkyltCathClgRef:Parent      = {  Parent( Name )  }
   "Set ExSkyltCathClgRef:Status"      CathedralCeiling:ExSkyltCathClgRef:Status      = {  "Existing"      }
   "Set ExSkyltCathClgRef:IsVerified"  CathedralCeiling:ExSkyltCathClgRef:IsVerified  = {  0  }
   "Set ExSkyltCathClgRef:Area"        CathedralCeiling:ExSkyltCathClgRef:Area        = {  AltClgExSkyltArea + 0.1  }
   "Adjust CathedralCeiling:Area for AltClgExSkyltArea"     CathedralCeiling:Area     = {
         if (Status != 2 .OR. IfValidAnd( AltClgExSkyltArea > 0 )==0) then  UNCHANGED
         else  max( 0.1, Area - AltClgExSkyltArea )  endif  }
   "Set ExSkyltCathClgRef:Construction"      CathedralCeiling:ExSkyltCathClgRef:Construction     = {  RuleLibrary( Cons, "ExistingSkylt Cathedral Ceiling" )  }
   "Set ExSkyltCathClgRef:RoofRise"          CathedralCeiling:ExSkyltCathClgRef:RoofRise         = {  RoofRise  }
   "Set ExSkyltCathClgRef:Orientation"       CathedralCeiling:ExSkyltCathClgRef:Orientation      = {  Orientation  }
   "Set ExSkyltCathClgRef:OrientationValue"  CathedralCeiling:ExSkyltCathClgRef:OrientationValue = {  OrientationValue  }
   "Set ExSkyltCathClgRef:RoofSolReflect"    CathedralCeiling:ExSkyltCathClgRef:RoofSolReflect   = {  RoofSolReflect  }
   "Set ExSkyltCathClgRef:RoofEmiss"         CathedralCeiling:ExSkyltCathClgRef:RoofEmiss        = {  RoofEmiss  }

   "switch Parent of each Existing Skylt (whose parent is Altered CathedralCeiling) to that object's ExSkyltCathClgRef"
      Skylt:Parent  = {
         if (Status != 1 .OR.
             ParentComponentType() != "CathedralCeiling") then  UNCHANGED
         else if (Parent( Status ) != 2 .OR.
                  ParentCompAssigned( ExSkyltCathClgRef ) < 1) then  UNCHANGED
         else  ExSkyltCathClgRef:Name
         endif endif  }
END
