;         then  EvalRulelist( "ForceDefault_CompactDistribution_MFam_DUT3", 1 )  else  UNCHANGED  endif  }
;   "Setup std design DwellUnitType:DHWSysRef[4]:CompactDistrib"   DwellUnitType:Action  = {
