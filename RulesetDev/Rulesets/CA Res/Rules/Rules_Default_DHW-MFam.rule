RULELIST  "ForceDefault_CompactDistribution_MFam" 1 0 1 1
; SAC 3/25/19 - no longer needed now that Fixtures Served specified @ dwelling, not system, level
;   ; first make changes to individual DHWSys COmpactDistrib settings
;   "Setup std design Fixtures Servedution inputs for indiv DHWSys objects"    DHWSys:Action  = {
;         if (IfValidAnd( IsExisting > 0.5 ) .OR. CentralDHW) then  UNCHANGED
;         else  EvalRulelist( "ForceDefault_CompactDistribution_SFam" )  endif  }

   ; SAC 4/1/19 - std design rules for MFam CompactDistrib
   "Set std design DwellUnitType:CompactDistrib"   DwellUnitType:CompactDistrib  =              ; StdDesignCompactDistYear - SAC 01/22/21   ; StdDesignCompactDistVal - SAC 01/27/21
      {  if ( IfValidAnd( IsStdDesignElecDHW > 0 ) .OR.
              (Proj:AllowStdDesignFuel > 0 .AND. IfValidAnd( Proj:StdDesignCompactDistrib > 0 ) .AND.
               (Proj:RunAbbrev == "s" .OR. (Proj:SMFFollowsStdDesignFuel .AND. Proj:RunAbbrev == "smf"))) )
         then  if (Proj:AllowStdDesignFuel > 0 .AND. IfValidAnd( Proj:StdDesignCompactDistrib > 0 ) .AND.
                   IfValidAnd( Proj:StdDesignCompactDistVal > 0 ) .AND. (Proj:RunAbbrev == "s" .OR. (Proj:SMFFollowsStdDesignFuel .AND. Proj:RunAbbrev == "smf")))
               then  switch (Proj:StdDesignCompactDistVal)
                        case  1.0 :  "not compact"
                        case  0.7 :  "Basic Credit"
                        default   :  "Expanded Credit (HERS req'd)"
                     endswitch
               else  switch (T24RClimateZoneCodeBase:ElecDHWCompactFactor( "ClimateZone", Proj:ClimateZone, "CodeBase", Proj:StdDesignCompactDistYear, "BldgType", Proj:BldgTypeTblStr ))
                        case  0.0 :  "not compact"    ; covers DRtg run
                        case  1.0 :  "not compact"
                        case  0.7 :  "Basic Credit"
                        default   :  "Expanded Credit (HERS req'd)"
                     endswitch
               endif
         else  T24RClimateZoneCodeBase:DHWCompactDistrib( "ClimateZone", Proj:ClimateZone, "CodeBase", Proj:StdDesignCompactDistYear, "BldgType", Proj:BldgTypeTblStr )
         endif  }
   "Set std design DwellUnitType:UsrSpecCmpctDist"    DwellUnitType:UsrSpecCmpctDist  = 
      {  if (CompactDistrib > 0) then  1  else  0  endif  }
   "Set std design DwellUnitType:UsrCompactFactor"    DwellUnitType:UsrCompactFactor  =         ; StdDesignCompactDistYear - SAC 01/22/21   ; StdDesignCompactDistVal - SAC 01/27/21
      {  if (UsrSpecCmpctDist < 1) then  UNDEFINED
         else if (Proj:AllowStdDesignFuel > 0 .AND. IfValidAnd( Proj:StdDesignCompactDistrib > 0 ) .AND.
                  IfValidAnd( Proj:StdDesignCompactDistVal > 0 ) .AND. (Proj:RunAbbrev == "s" .OR. (Proj:SMFFollowsStdDesignFuel .AND. Proj:RunAbbrev == "smf")))
         then  Proj:StdDesignCompactDistVal
         else  T24RClimateZoneCodeBase:ElecDHWCompactFactor( "ClimateZone", Proj:ClimateZone, "CodeBase", Proj:StdDesignCompactDistYear, "BldgType", Proj:BldgTypeTblStr )
         endif endif  }

;   ; then mods to DwellingUnitType data
