         if (IfValidAnd( IsStdDesignElec > 0 )==0) then  UNCHANGED  else  UNDEFINED  endif  }
   "Blast std design DHWSys:DWHRSysRef[4]"   DHWSys:DWHRSysRef[4]  = {
         if (IfValidAnd( IsStdDesignElec > 0 )==0) then  UNCHANGED  else  UNDEFINED  endif  }
   "Blast std design DHWSys:DWHRSysRef[5]"   DHWSys:DWHRSysRef[5]  = {
         if (IfValidAnd( IsStdDesignElec > 0 )==0) then  UNCHANGED  else  UNDEFINED  endif  }
   "Blast std design DHWSys:DWHRSysRef[6]"   DHWSys:DWHRSysRef[6]  = {
         if (IfValidAnd( IsStdDesignElec > 0 )==0) then  UNCHANGED  else  UNDEFINED  endif  }
