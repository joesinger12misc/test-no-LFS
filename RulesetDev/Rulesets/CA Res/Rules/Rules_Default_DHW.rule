RULELIST  "ForceDefault_ShowerDrainHeatRecov_SFam" 1 0 1 1
   ; incorporated StdDesignDrnWtrHtRecYear into table look-ups - SAC 01/22/21
   "Blast Proj:ShwrDRAINHREFF"   Proj:ShwrDRAINHREFF  = {  0  }

   "Set std design DHWSys:TotShowersServed"  DHWSys:TotShowersServed  = {  1 + int( (ValidOr( Proj:NumBedrooms, 1 )+1)/2.5 )  }
   "Set std design DHWSys:DWHRSysRef[1]"     DHWSys:DWHRSysRef[1]  = {
         if (IfValidAnd( IsStdDesignElec > 0 )==0) then  UNCHANGED
         else  Proj:StdElecDWHRSysRef[1]  endif  }
   "Set std design DHWSys:DWHRSysShowers[1]" DHWSys:DWHRSysShowers[1]  = {  TotShowersServed *
         T24RClimateZoneCodeBase:ElecDHWDrnWtrHtRecovShwrSrvd( "ClimateZone", Proj:ClimateZone, "CodeBase", Proj:StdDesignDrnWtrHtRecYear, "BldgType", Proj:BldgTypeTblStr ) / 100  }
;   "Set std design DHWSys:DWHRSysConfig[1]"  DHWSys:DWHRSysConfig[1]  = {
;         T24RClimateZoneCodeBase:ElecDHWDrnWtrHtRecovConfig( "ClimateZone", Proj:ClimateZone, "CodeBase", Proj:StdDesignDrnWtrHtRecYear, "BldgType", Proj:BldgTypeTblStr )  }
; SAC 3/5/19 - replaced setting of DWHRSysConfig w/ eval of rulelists to setup new DWHR details (tic #1072)
   "Call subordinate rulelist to setup ElecDHWDrnWtrHtRecov details"    DHWSys:Action  = {
         switch (T24RClimateZoneCodeBase:ElecDHWDrnWtrHtRecovConfig( "ClimateZone", Proj:ClimateZone, "CodeBase", Proj:StdDesignDrnWtrHtRecYear, "BldgType", Proj:BldgTypeTblStr ))
            case "Equal Flow"             :  EvalRulelist( "ForceDefault_ShowerDrainHeatRecov_SFam_Equal", 1 )
            case "Unequal - Water Heater" :  EvalRulelist( "ForceDefault_ShowerDrainHeatRecov_SFam_UneqWHtr", 1 )
