                                          <xsd:documentation source="FieldText">HVAC System Name</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G04_ThermalZoneFloorArea" type="hvac:ThermalZoneFloorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G05_FloorToCeilingHeight" type="com:FloorToCeilingHeight" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Avg. Ceiling Height</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G06_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System 1</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G07_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heating System 2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Required value if there is more than one water heating system in the zone, else value is N/A stored in NotApplicableMessage.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText"></xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input if there is more than one water heating system in the zone, else value is N/A stored in NotApplicableMessage.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">User Defined Fields</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:element name="GsfUserDefined_FieldName" type="com:FieldName" >
                                       </xsd:element>
                                       <xsd:element name="GsfUserDefined_FieldValue" type="com:FieldValue" >
                                       </xsd:element>
                                       <xsd:element name="GsfUserDefined_FieldValueType" type="com:FieldValueType" >
                                       </xsd:element>
                                    </xsd:sequence>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TableZoneMF" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Complete this table for Multi- Family</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_ThermalZoneName" type="com:ThermalZoneName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Name</xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">Complete this table for Multi- Family</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G02_ZoneConditioningType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Type</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:ZoneConditioningType">
                                             <xsd:enumeration value="Conditioned"/>
                                             <xsd:enumeration value="Living"/>
                                             <xsd:enumeration value="Sleeping"/>
                                             <xsd:enumeration value="Unconditioned"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="G03_ThermalZoneFloorArea" type="hvac:ThermalZoneFloorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G04_FloorToCeilingHeight" type="com:FloorToCeilingHeight" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Avg. Ceiling Height</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G05_DwellingUnitCount" type="com:DwellingUnitCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Number of Dweilling Units</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">User Defined Fields</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:element name="GmfUserDefined_FieldName" type="com:FieldName" >
                                       </xsd:element>
                                       <xsd:element name="GmfUserDefined_FieldValue" type="com:FieldValue" >
                                       </xsd:element>
                                       <xsd:element name="GmfUserDefined_FieldValueType" type="com:FieldValueType" >
                                       </xsd:element>
                                    </xsd:sequence>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:choice>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">DWELLING UNIT INFORMATION</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if Building type in General Information section equals LowriseMultiFamily</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
