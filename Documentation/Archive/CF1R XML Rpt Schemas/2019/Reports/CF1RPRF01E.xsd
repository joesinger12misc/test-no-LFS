                                          <xsd:documentation source="CalculationsAndRules">This value should match one of a value in the Zone Information section column 01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">User Defined Fields</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:element name="HUserDefined_FieldName" type="com:FieldName" >
                                       </xsd:element>
                                       <xsd:element name="HUserDefined_FieldValue" type="com:FieldValue" >
                                       </xsd:element>
                                       <xsd:element name="HUserDefined_FieldValueType" type="com:FieldValueType" >
