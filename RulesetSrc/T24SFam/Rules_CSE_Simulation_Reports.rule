                                     else  if (IfValidAnd( ActiveHVACSystem:ElecHeatAreaServed > 1 )==0) then  UNCHANGED
                                     else  AddCSEReportCol( "none",  Format( "@Meter[%c%s%c].D.htg/1000 colHead=%cHtgE%c colDec=0 colWid=6",     34, Proj:CSE_ElecMETER:Name, 34, 34, 34 ) )  endif endif  }
   "ZEBday HtgG    col"   OtherZone:Action = {  if (Proj:Fuel_HaveNatGas < 0.5 .OR. LocalCompAssigned( ActiveHVACSystem ) < 1) then  UNCHANGED
                                     else  if (IfValidAnd( ActiveHVACSystem:GasHeatAreaServed > 1 )==0) then  UNCHANGED
                                     else  AddCSEReportCol( "none",  Format( "@Meter[%c%s%c].D.htg/1000 colHead=%cHtgG%c colDec=0 colWid=6",     34, Proj:CSE_NatGasMETER:Name, 34, 34, 34 ) )  endif endif  }
