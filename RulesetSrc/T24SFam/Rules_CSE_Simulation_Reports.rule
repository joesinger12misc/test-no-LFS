                                     else  if (IfValidAnd( ActiveHVACSystem:GasHeatAreaServed > 1 )==0) then  UNCHANGED
                                     else  AddCSEReportCol( "none",  Format( "@Meter[%c%s%c].D.htg/1000 colHead=%cHtgP%c colDec=0 colWid=6",     34, Proj:CSE_OtherFuelMETER:Name, 34, 34, 34 ) )  endif endif  }
   ; cooling energy use
   "ZEBday FfanCE  col"   OtherZone:Action = {  if (LocalCompAssigned( ActiveHVACSystem ) < 1) then  UNCHANGED
                                     else  if (IfValidAnd( ActiveHVACSystem:ClFlrAreaServed[1] > 1 )==0) then  UNCHANGED
                                     else  AddCSEReportCol( "none",  Format( "@Meter[%c%s%c].D.fanc/1000 colHead=%cFfanCE%c colDec=0 colWid=6",  34, Proj:CSE_ElecMETER:Name, 34, 34, 34 ) )  endif endif  }
   "ZEBday ClgE    col"   OtherZone:Action = {  if (LocalCompAssigned( ActiveHVACSystem ) < 1) then  UNCHANGED
                                     else  if (IfValidAnd( ActiveHVACSystem:ClFlrAreaServed[1] > 1 )==0) then  UNCHANGED
                                     else  AddCSEReportCol( "none",  Format( "@Meter[%c%s%c].D.clg/1000 colHead=%cClgE%c colDec=0 colWid=6",     34, Proj:CSE_ElecMETER:Name, 34, 34, 34 ) )  endif endif  }
   ; duct results
   "ZEBday dct SupT* col" OtherZone:Action = {  if (IfValidAnd( ActiveHVACSystem:cseRSYS:SupplyDuct:dsDiameter > 0 )==0) then  UNCHANGED
                                     else  AddCSEReportCol( "none",  Format( "%c     %c   colHead=%cSupT*%c colDec=1 colWid=5",   34, 34, 34, 34 ) )  endif  }
   "ZEBday dct SupW* col" OtherZone:Action = {  if (IfValidAnd( ActiveHVACSystem:cseRSYS:SupplyDuct:dsDiameter > 0 )==0) then  UNCHANGED
                                     else  AddCSEReportCol( "none",  Format( "%c      %c  colHead=%cSupW*%c colDec=5 colWid=6",   34, 34, 34, 34 ) )  endif  }
   "ZEBday dct Scfm* col" OtherZone:Action = {  if (IfValidAnd( ActiveHVACSystem:cseRSYS:SupplyDuct:dsDiameter > 0 )==0) then  UNCHANGED
                                     else  AddCSEReportCol( "none",  Format( "%c     %c   colHead=%cScfm*%c colDec=0 colWid=5",   34, 34, 34, 34 ) )  endif  }
