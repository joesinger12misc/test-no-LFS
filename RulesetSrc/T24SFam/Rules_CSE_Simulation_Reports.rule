RULELIST "CSE_SimulationPrep_Reports" 1 0 0 1
;   Rules which are executed immediately prior to performing a CSE simulation - covering report & ReportCol objects only
;*************************************************************************

   ; ZDD - Zone Data Dump reports
   "Setup ZDD reports"  Proj:Action  = {
         if (CSERpt_ZnDataDump < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_ZDD" )  endif  }

   ; ZEB - Zone Energy Balance (monthly) reports
   "Setup ZEB-Mo reports"  Zone:Action  = {
         if (Proj:CSERpt_ZnEnergy < 1 .OR.
             IfValidAnd( FloorArea > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_ZEBMo", 1 )  endif  }
   "Setup ZEB-Mo reports"  OtherZone:Action  = {
         if (Proj:CSERpt_ZnEnergy < 1 .OR.
             IfValidAnd( Area > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_ZEBMo_OtherZone", 1 )  endif  }
   ; populate ZEBMo ReportCol data for various referenced objects
   "Resolve ZEBMo data pertaining to adjacent zones"  cseREPORTCOL:colVal_x  = {
           if (LocalCompAssigned( ZEBMo_AtticRef    )) then  Format( "@znRes[%c%s%c].M.tAir colHead=%cTat%g%c colWid=4 colDec=1", 34,    ZEBMo_AtticRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBMo_AtticRef    ), 34 )
      else if (LocalCompAssigned( ZEBMo_GarageRef   )) then  Format( "@znRes[%c%s%c].M.tAir colHead=%cTgr%g%c colWid=4 colDec=1", 34,   ZEBMo_GarageRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBMo_GarageRef   ), 34 )
      else if (LocalCompAssigned( ZEBMo_CrawlSpcRef )) then  Format( "@znRes[%c%s%c].M.tAir colHead=%cTcr%g%c colWid=4 colDec=1", 34, ZEBMo_CrawlSpcRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBMo_CrawlSpcRef ), 34 )
      else if (LocalCompAssigned( ZEBT_AtticRef     )) then  Format( "@znRes[%c%s%c].Y.tAir colHead=%cTat%g%c colWid=4 colDec=1", 34,     ZEBT_AtticRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBT_AtticRef     ), 34 )
      else if (LocalCompAssigned( ZEBT_GarageRef    )) then  Format( "@znRes[%c%s%c].Y.tAir colHead=%cTgr%g%c colWid=4 colDec=1", 34,    ZEBT_GarageRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBT_GarageRef    ), 34 )
      else if (LocalCompAssigned( ZEBT_CrawlSpcRef  )) then  Format( "@znRes[%c%s%c].Y.tAir colHead=%cTcr%g%c colWid=4 colDec=1", 34,  ZEBT_CrawlSpcRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBT_CrawlSpcRef  ), 34 )
      else  UNCHANGED  endif endif endif endif endif endif  }

   ; HVAC - HVAC System Characteristics reports
   "Setup HVAC reports"  HVACSys:Action  = {
         if (Proj:CSERpt_HVACDescrip < 1 .OR.
             IfValidAnd( FloorAreaServed[1] > 1 )==0 .OR. LocalCompAssigned( cseRSYS ) < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_HVAC", 1 )  endif  }

   ; HVAC - Heat Pump Heating (3 day hourly) reports - SAC 11/01/20
   "Setup HVAC HtPumpHtg reports"  HVACSys:Action  = {
         if (Proj:CSERpt_HtPumpHtg < 1 .OR. IfValidAnd( FloorAreaServed[1] > 1 )==0 .OR.
             LocalCompAssigned( cseRSYS ) < 1 .OR. IfValidAnd( Type = 2 )==0) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_HtPumpHtg", 1 )  endif  }

   ; Meter - monthly metered energy use reports
   "Create Monthly Elec Meter report"        Proj:CSERpt_Mtr_ElecRef[1]  = {
         if (CSERpt_Mtr_Elec < 0.5 .OR. LocalCompAssigned( CSE_ElecMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "elec mtr rpt" )  endif  }
   "Create Monthly NatGas Meter report"      Proj:CSERpt_Mtr_NGasRef[1]  = {
         if (CSERpt_Mtr_NGas < 0.5 .OR. Fuel_HaveNatGas < 0.5 .OR. LocalCompAssigned( CSE_NatGasMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "natgas mtr rpt" )  endif  }
   "Create Monthly Propane Meter report"     Proj:CSERpt_Mtr_OthrRef[1]  = {
         if (CSERpt_Mtr_Othr < 0.5 .OR. Fuel_HaveOther < 0.5 .OR. LocalCompAssigned( CSE_OtherFuelMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "propane mtr rpt" )  endif  }

   "Setup monthly elec meter report"         Proj:CSERpt_Mtr_ElecRef[1]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=Month  rpBtuSf=1000", 34, CSE_ElecMETER:Name, 34 )  }
   "Setup monthly natgas meter report"       Proj:CSERpt_Mtr_NGasRef[1]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=Month  rpBtuSf=1000", 34, CSE_NatGasMETER:Name, 34 )  }
   "Setup monthly propane meter report"      Proj:CSERpt_Mtr_OthrRef[1]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=Month  rpBtuSf=1000", 34, CSE_OtherFuelMETER:Name, 34 )  }


; DD - COOLING DESIGN DAY Reports ---

   ; ZEB - Zone Energy Balance (hourly cooling design day) reports
   "Setup ZEB-DD reports"  Zone:Action  = {
         if (Proj:CSERpt_ZnEnergy_DD < 1 .OR.
             IfValidAnd( FloorArea > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_ZEB_DD", 1 )  endif  }
   "Setup ZEB-DD reports"  OtherZone:Action  = {
         if (Proj:CSERpt_ZnEnergy_DD < 1 .OR.
             IfValidAnd( Area > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_ZEB_DD_OtherZone", 1 )  endif  }

   ; DD Meter - hourly metered energy use reports for the cooling design day
   "Create DD Hourly Elec Meter report"        Proj:CSERpt_Mtr_ElecRef[2]  = {
         if (CSERpt_Mtr_Elec_DD < 0.5 .OR. LocalCompAssigned( CSE_ElecMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "DD elec mtr rpt" )  endif  }
   "Create DD Hourly NatGas Meter report"      Proj:CSERpt_Mtr_NGasRef[2]  = {
         if (CSERpt_Mtr_NGas_DD < 0.5 .OR. Fuel_HaveNatGas < 0.5 .OR. LocalCompAssigned( CSE_NatGasMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "DD natgas mtr rpt" )  endif  }
   "Create DD Hourly Propane Meter report"     Proj:CSERpt_Mtr_OthrRef[2]  = {
         if (CSERpt_Mtr_Othr_DD < 0.5 .OR. Fuel_HaveOther < 0.5 .OR. LocalCompAssigned( CSE_OtherFuelMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "DD propane mtr rpt" )  endif  }

   "Setup DD hourly elec meter report"         Proj:CSERpt_Mtr_ElecRef[2]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=hour  rpBtuSf=1000  rpDayBeg=DD1", 34, CSE_ElecMETER:Name, 34 )  }
   "Setup DD hourly natgas meter report"       Proj:CSERpt_Mtr_NGasRef[2]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=hour  rpBtuSf=1000  rpDayBeg=DD1", 34, CSE_NatGasMETER:Name, 34 )  }
   "Setup DD hourly propane meter report"      Proj:CSERpt_Mtr_OthrRef[2]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=hour  rpBtuSf=1000  rpDayBeg=DD1", 34, CSE_OtherFuelMETER:Name, 34 )  }


; CD - EXTREME COOLING DAY Reports ---

   ; ZEB - Zone Energy Balance (hourly extreme cooling day) reports
   "Setup ZEB-CD reports"  Zone:Action  = {
         if (Proj:CSERpt_ZnEnergy_CD < 1 .OR.
             IfValidAnd( FloorArea > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_ZEB_CD", 1 )  endif  }
   "Setup ZEB-CD reports"  OtherZone:Action  = {
         if (Proj:CSERpt_ZnEnergy_CD < 1 .OR.
             IfValidAnd( Area > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_ZEB_CD_OtherZone", 1 )  endif  }

   ; CD Meter - hourly metered energy use reports for the extreme cooling day
   "Create CD Hourly Elec Meter report"        Proj:CSERpt_Mtr_ElecRef[3]  = {
         if (CSERpt_Mtr_Elec_CD < 0.5 .OR. LocalCompAssigned( CSE_ElecMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "CD elec mtr rpt" )  endif  }
   "Create CD Hourly NatGas Meter report"      Proj:CSERpt_Mtr_NGasRef[3]  = {
         if (CSERpt_Mtr_NGas_CD < 0.5 .OR. Fuel_HaveNatGas < 0.5 .OR. LocalCompAssigned( CSE_NatGasMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "CD natgas mtr rpt" )  endif  }
   "Create CD Hourly Propane Meter report"     Proj:CSERpt_Mtr_OthrRef[3]  = {
         if (CSERpt_Mtr_Othr_CD < 0.5 .OR. Fuel_HaveOther < 0.5 .OR. LocalCompAssigned( CSE_OtherFuelMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "CD propane mtr rpt" )  endif  }

   "Setup CD hourly elec meter report"         Proj:CSERpt_Mtr_ElecRef[3]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=hour  rpBtuSf=1000  rpDayBeg=CD1", 34, CSE_ElecMETER:Name, 34 )  }
   "Setup CD hourly natgas meter report"       Proj:CSERpt_Mtr_NGasRef[3]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=hour  rpBtuSf=1000  rpDayBeg=CD1", 34, CSE_NatGasMETER:Name, 34 )  }
   "Setup CD hourly propane meter report"      Proj:CSERpt_Mtr_OthrRef[3]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=hour  rpBtuSf=1000  rpDayBeg=CD1", 34, CSE_OtherFuelMETER:Name, 34 )  }


; HD - EXTREME HEATING DAY Reports ---

   ; ZEB - Zone Energy Balance (hourly extreme heating day) reports
   "Setup ZEB-HD reports"  Zone:Action  = {
         if (Proj:CSERpt_ZnEnergy_HD < 1 .OR.
             IfValidAnd( FloorArea > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_ZEB_HD", 1 )  endif  }
   "Setup ZEB-HD reports"  OtherZone:Action  = {
         if (Proj:CSERpt_ZnEnergy_HD < 1 .OR.
             IfValidAnd( Area > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNCHANGED
         else  EvalRulelist( "CSE_SimulationPrep_Reports_ZEB_HD_OtherZone", 1 )  endif  }

   ; HD Meter - hourly metered energy use reports for the extreme heating day
   "Create HD Hourly Elec Meter report"        Proj:CSERpt_Mtr_ElecRef[4]  = {
         if (CSERpt_Mtr_Elec_HD < 0.5 .OR. LocalCompAssigned( CSE_ElecMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "HD elec mtr rpt" )  endif  }
   "Create HD Hourly NatGas Meter report"      Proj:CSERpt_Mtr_NGasRef[4]  = {
         if (CSERpt_Mtr_NGas_HD < 0.5 .OR. Fuel_HaveNatGas < 0.5 .OR. LocalCompAssigned( CSE_NatGasMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "HD natgas mtr rpt" )  endif  }
   "Create HD Hourly Propane Meter report"     Proj:CSERpt_Mtr_OthrRef[4]  = {
         if (CSERpt_Mtr_Othr_HD < 0.5 .OR. Fuel_HaveOther < 0.5 .OR. LocalCompAssigned( CSE_OtherFuelMETER ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, "HD propane mtr rpt" )  endif  }

   "Setup HD hourly elec meter report"         Proj:CSERpt_Mtr_ElecRef[4]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=hour  rpBtuSf=1000  rpDayBeg=HD1", 34, CSE_ElecMETER:Name, 34 )  }
   "Setup HD hourly natgas meter report"       Proj:CSERpt_Mtr_NGasRef[4]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=hour  rpBtuSf=1000  rpDayBeg=HD1", 34, CSE_NatGasMETER:Name, 34 )  }
   "Setup HD hourly propane meter report"      Proj:CSERpt_Mtr_OthrRef[4]:rpType_x  = {
         Format( "MTR  rpMeter=%c%s%c  rpFreq=hour  rpBtuSf=1000  rpDayBeg=HD1", 34, CSE_OtherFuelMETER:Name, 34 )  }

   ; populate ZEBDD/ZEBCD/ZEBHD ReportCol data for various referenced objects
   "Resolve ZEBDD data pertaining to adjacent zones"  cseREPORTCOL:colVal_x  = {
           if (LocalCompAssigned( ZEBDD_AtticRef     )) then  Format( "@znRes[%c%s%c].H.tAir colHead=%cTat%g%c colWid=4 colDec=1", 34,     ZEBDD_AtticRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBDD_AtticRef     ), 34 )
      else if (LocalCompAssigned( ZEBDD_GarageRef    )) then  Format( "@znRes[%c%s%c].H.tAir colHead=%cTgr%g%c colWid=4 colDec=1", 34,    ZEBDD_GarageRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBDD_GarageRef    ), 34 )
      else if (LocalCompAssigned( ZEBDD_CrawlSpcRef  )) then  Format( "@znRes[%c%s%c].H.tAir colHead=%cTcr%g%c colWid=4 colDec=1", 34,  ZEBDD_CrawlSpcRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBDD_CrawlSpcRef  ), 34 )
      else if (LocalCompAssigned( ZEBDDt_AtticRef    )) then  Format( "@znRes[%c%s%c].D.tAir colHead=%cTat%g%c colWid=4 colDec=1", 34,    ZEBDDt_AtticRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBDDt_AtticRef    ), 34 )
      else if (LocalCompAssigned( ZEBDDt_GarageRef   )) then  Format( "@znRes[%c%s%c].D.tAir colHead=%cTgr%g%c colWid=4 colDec=1", 34,   ZEBDDt_GarageRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBDDt_GarageRef   ), 34 )
      else if (LocalCompAssigned( ZEBDDt_CrawlSpcRef )) then  Format( "@znRes[%c%s%c].D.tAir colHead=%cTcr%g%c colWid=4 colDec=1", 34, ZEBDDt_CrawlSpcRef:CSE_ZONE:Name, 34, 34, ComponentIndex( ZEBDDt_CrawlSpcRef ), 34 )
      else  UNCHANGED  endif endif endif endif endif endif  }

END
;*************************************************************************

;*************************************************************************
RULELIST "CSE_SimulationPrep_Reports_ZDD" 1 0 0 1

   "Create Zone:ZoneRpt_ZDD"     Zone:ZoneRpt_ZDD  = {
         if (IfValidAnd( FloorArea > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, substr( Name, 0, 47 ) + "-zdd" )  endif  }

   "Create OtherZone:ZoneRpt_ZDD"     OtherZone:ZoneRpt_ZDD  = {
         if (IfValidAnd( Area > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, substr( Name, 0, 47 ) + "-zdd" )  endif  }

   "Create Attic:ZoneRpt_ZDD"    Attic:ZoneRpt_ZDD  = {
         if (IfValidAnd( FloorArea > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, substr( Name, 0, 47 ) + "-zdd" )  endif  }

   "Create CrawlSpace:ZoneRpt_ZDD"  CrawlSpace:ZoneRpt_ZDD  = {
         if (IfValidAnd( Area > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, substr( Name, 0, 47 ) + "-zdd" )  endif  }

   "Create Garage:ZoneRpt_ZDD"   Garage:ZoneRpt_ZDD  = {
         if (IfValidAnd( Area > 1 )==0 .OR. LocalCompAssigned( CSE_ZONE ) < 1) then  UNDEFINED
         else  CreateComp( cseREPORT, substr( Name, 0, 47 ) + "-zdd" )  endif  }

   "Setup Zone:ZoneRpt_ZDD"         Zone:ZoneRpt_ZDD:rpType_x        = {  Format( "ZDD  rpZone = %c%s%c", 34, CSE_ZONE:Name, 34 )  }
   "Setup OtherZone:ZoneRpt_ZDD"    OtherZone:ZoneRpt_ZDD:rpType_x   = {  Format( "ZDD  rpZone = %c%s%c", 34, CSE_ZONE:Name, 34 )  }
   "Setup Attic:ZoneRpt_ZDD"        Attic:ZoneRpt_ZDD:rpType_x       = {  Format( "ZDD  rpZone = %c%s%c", 34, CSE_ZONE:Name, 34 )  }
   "Setup CrawlSpace:ZoneRpt_ZDD"   CrawlSpace:ZoneRpt_ZDD:rpType_x  = {  Format( "ZDD  rpZone = %c%s%c", 34, CSE_ZONE:Name, 34 )  }
   "Setup Garage:ZoneRpt_ZDD"       Garage:ZoneRpt_ZDD:rpType_x      = {  Format( "ZDD  rpZone = %c%s%c", 34, CSE_ZONE:Name, 34 )  }

END
;*************************************************************************


;*************************************************************************
RULELIST "CSE_SimulationPrep_Reports_ZEBMo" 1 0 0 1

; MONTHLY records
   "Create Zone:ZoneRpt_ZEB"     Zone:ZoneRpt_ZEB  = {   CreateComp( cseREPORT, substr( Name, 0, 47 ) + "-zebm" )  }
   "Setup Zone:ZoneRpt_ZEB"      Zone:ZoneRpt_ZEB:rpType_x  = {
      Format( "UDT rpBtuSf=1000  rpFreq=Month  rpCPL=160  rpTitle=%cMonthly Energy (kBtu + into %s, except E = Energy Consumed)%c", 34, CSE_ZONE:Name, 34 )  }

   "ZEBm month col"      Zone:Action = {  AddCSEReportCol( "none",             Format( "$month  colHead=%cmo%c      colWid=2", 34, 34 ) )  }
   "ZEBm attic col(s)"   Zone:Action = {  AddCSEReportCol( "AdjacentAttics",   cseREPORTCOL:ZEBMo_AtticRef )  }
   "ZEBm garage col(s)"  Zone:Action = {  AddCSEReportCol( "AdjacentGarages",  cseREPORTCOL:ZEBMo_GarageRef )  }
   "ZEBm crl spc col(s)" Zone:Action = {  AddCSEReportCol( "AdjacentCrlSpcs",  cseREPORTCOL:ZEBMo_CrawlSpcRef )  }

   "ZEBm Tin   col"      Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.tAir colHead=%cTin%c colWid=4 colDec=1",   34, CSE_ZONE:Name, 34, 34, 34 ) )  }
   "ZEBm Trad  col"      Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.tRad colHead=%cTrad%c colWid=4 colDec=1",  34, CSE_ZONE:Name, 34, 34, 34 ) )  }
   "ZEBm Slr   col"      Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.qSlr/1000 colHead=%cSlr%c colDec=0",       34, CSE_ZONE:Name, 34, 34, 34 ) )  }

   "ZEBm qsIg      col"  Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.qsIg/1000 colHead=%cIntGs%c colDec=0",     34, CSE_ZONE:Name, 34, 34, 34 ) )  } 
   "ZEBm qlIg      col"  Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.qlIg/1000 colHead=%cIntGl%c colDec=0",     34, CSE_ZONE:Name, 34, 34, 34 ) )  } 
   "ZEBm qCond     col"  Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.qCond/1000 colHead=%cCond%c colDec=0",     34, CSE_ZONE:Name, 34, 34, 34 ) )  } 
   "ZEBm qMass     col"  Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.qMass/1000 colHead=%cSurf%c colDec=0",     34, CSE_ZONE:Name, 34, 34, 34 ) )  } 
   "ZEBm eqfVentHr col"  Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.eqfVentHr colHead=%cVntHr%c colDec=0 colWid=5",   34, CSE_ZONE:Name, 34, 34, 34 ) )  } 
   "ZEBm qsIz      col"  Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.qsIz/1000 colHead=%csInfVnt%c colDec=0 colWid=7", 34, CSE_ZONE:Name, 34, 34, 34 ) )  } 
   "ZEBm ivAirX    col"  Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.ivAirX colHead=%cACiv%c colWid=4 colDec=2", 34, CSE_ZONE:Name, 34, 34, 34 ) )  } 
   "ZEBm qvMech    col"  Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.qvMech/1000 colHead=%cOAV%c colDec=0",      34, CSE_ZONE:Name, 34, 34, 34 ) )  } 
   "ZEBm qhMech    col"  Zone:Action = {  AddCSEReportCol( "none",  Format( "@znRes[%c%s%c].M.qhMech/1000 colHead=%cHload%c colDec=0",    34, CSE_ZONE:Name, 34, 34, 34 ) )  } 
