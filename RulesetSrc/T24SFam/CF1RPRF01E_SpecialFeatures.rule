;************************************************************************* - PVException*
RULELIST "CF1RPRF01E_SpecialFeature_PVException"  1 0 0 1
     "Set E01_SpecialFeatureCategory"  cf1rSpecialFeature:E01_SpecialFeatureCategory  =                     ; added excpt 7 & 8 - SAC 03/01/21 (tic #1245)   ; updated for 2022 exceptions - SAC 05/23/22 (tic #1292)
        {  switch (Proj:ReducedPVReqExcept)
              case    1 :  "PVExceptionEffectSolarAccess"
              case    2 :  "PVExceptionSolarAccessAndHomeArea"
              case    3 :  "PVException2HabitableStories"
              case    4 :  "PVException3HabitableStories"
              case    5 :  "PVExceptionPriorSolarReadyZone"
              case    6 :  "PVExceptionAB178EmergencyArea"
              case    7 :  "PVExceptionSection10_109k"
              case    8 :  "PVExceptionSnowLoad"
              case 2201 :  "PVExceptionEffectSolarAccess"         ; was: "PVException2201SARA"
              case 2202 :  "PVExceptionSolarAccessAndHomeArea"    ; was: "PVException2202MinReqd"
              case 2203 :  "PVExceptionSnowLoad"                  ; was: "PVException2203SnowLoad"
              case 2204 :  "PVExceptionPlanningDeptApproved"      ; was: "PVException2204DeptApproval"
              case 2206 :  "PVExceptionEffectSolarAccessAbove80"  ; new - SAC 12/15/22
              default   :  UNDEFINED
           endswitch  }
     "Set E01b_SpecialFeatureCategoryString"  cf1rSpecialFeature:E01b_SpecialFeatureCategoryString  =       ; revised excpt 6 msg & added excpt 7 & 8 - SAC 03/01/21 (tic #1245)
         { switch (Proj:ReducedPVReqExcept)
              case    1 :  Format( "PV exception 1: Effective solar access < 80 ft2 (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) )
              case    2 :  Format( "PV exception 2: Smaller of solar access and home area-based size (CZ 15 only) (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) )
              case    3 :  Format( "PV exception 3: 2 habitable stories (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) )
              case    4 :  Format( "PV exception 4: 3 habitable stories (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) )
              case    5 :  Format( "PV exception 5: 80-200 ft2 solar ready zone approved before 1/1/20 (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) )
              case    6 :  Format( "PV exception 6: Declared emergency area (prior to 1/1/20) (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) ) 
              case    7 :  Format( "PV exception 7: Section 10-109(k) determination (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) ) 
              case    8 :  Format( "PV exception 8: Excessive snow load (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) ) 
              case 2201 :  Format( "PV exception 1: No PV required if the SARA < 80 ft2 (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) )
              case 2202 :  Format( "PV exception 2: No PV required when minimum PV size (Section 150.1(c)14) < 1.8 kWdc (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) )
              case 2203 :  Format( "PV exception 3: Excessive snow load (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) ) 
              case 2204 :  Format( "PV exception 4: Building approved by planning dept prior to 1/1/20 (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) ) 
              case 2206 :  Format( "PV exception 5: PV size limited by Solar Access Roof Area (>= 80 ft2) (%g kW)", ValidOr( Proj:ReducedPVReqValue, 0 ) )  ; new - SAC 12/15/22
              Default   :  UNDEFINED
           endswitch  }
     "Set E01c_SpecialFeatureCategoryData"  cf1rSpecialFeature:E01c_SpecialFeatureCategoryData  = {
           if (IfValidAnd( Proj:ReducedPVReqValue >= 0 )==0) then  UNDEFINED
           else  FltToStr( Proj:ReducedPVReqValue, 2, 0, 0, 1 )  endif  }
END
