RULELIST "CF1RPRF01E_Proposed_SpecialFeatures"  1 0 0 1

;"store BEMProc details"		Proj:Action = {  StoreBEMProc( Format( "b4 CF1R Prop SpclFtrs-%s", RunAbbrev ), 0 )  }

  ; Section_E Manual Field population
   "Create & initialize new cf1rSection_E object"
      cf1rCF1RPRF01E:Action  = {  CreateChildren( cf1rSection_E, 1, "CF1R SecE" )  }     ; no init needed

   "Create cf1rSpecialFeature object - ZonalHeatingControls"
      cf1rSection_E:Action  = {
         if (IfValidAnd( Proj:SpeclFeatrs:ZonalControl != 0 ))
         then  CreateChildren( cf1rSpecialFeature, 1, "CF1R SecE SclFtr",
                               "CF1RPRF01E_SpecialFeature_ZonalHeatingControls" )
         else  UNCHANGED  endif  }
   "Create cf1rSpecialFeature object - PVCommunitySolar"          ; SAC 6/12/20
      cf1rSection_E:Action  = {
         if (IfValidAnd( Proj:CommunitySolarProjID > 0 ))
         then  CreateChildren( cf1rSpecialFeature, 1, "CF1R SecE SclFtr",
                               "CF1RPRF01E_SpecialFeature_PVCommunitySolar" )
         else  UNCHANGED  endif  }
   "Create cf1rSpecialFeature object - PV2kWSystem"         ; disabled for EAA & AddAlone run scopes - SAC 09/04/20
      cf1rSection_E:Action  = {
         if (Proj:RunScope == 1 .AND. Proj:IsAddAlone == 0 .AND.
             IfValidAnd( Proj:SpeclFeatrs:PVSysCredit != 0 ) .AND. IfValidAnd( Proj:CommunitySolarProjID > 0 )==0)
         then  CreateChildren( cf1rSpecialFeature, 1, "CF1R SecE SclFtr",
                               "CF1RPRF01E_SpecialFeature_PV2kWSystem" )
         else  UNCHANGED  endif  }
   "Create cf1rSpecialFeature object - PVModuleTypePremium"    ; SAC 5/18/20 (tic #1213)
      cf1rSection_E:Action  = {
         if (Proj:RunScope != 1 .OR. Proj:IsAddAlone > 0 .OR.
             IfValidAnd( Proj:PVWDCSysTotal > 0 )==0 .OR. IfValidAnd( Proj:CommunitySolarProjID > 0 ) .OR.
             (IfValidAnd( Proj:ReducedPVReq > 0 ) .AND. IfValidAnd( Proj:UseReducedPVReqVal = 0 ))) then  UNCHANGED
         else if ( (                         IfValidAnd( Proj:PVWDCSysSize[1] > 0 ) .AND. Proj:PVWModuleType[1] = 2) .OR.
                   (Proj:PVWInputs > 0 .AND. IfValidAnd( Proj:PVWDCSysSize[2] > 0 ) .AND. Proj:PVWModuleType[2] = 2) .OR.
                   (Proj:PVWInputs > 0 .AND. IfValidAnd( Proj:PVWDCSysSize[3] > 0 ) .AND. Proj:PVWModuleType[3] = 2) .OR.
                   (Proj:PVWInputs > 0 .AND. IfValidAnd( Proj:PVWDCSysSize[4] > 0 ) .AND. Proj:PVWModuleType[4] = 2) .OR.
                   (Proj:PVWInputs > 0 .AND. IfValidAnd( Proj:PVWDCSysSize[5] > 0 ) .AND. Proj:PVWModuleType[5] = 2) )
         then  CreateChildren( cf1rSpecialFeature, 1, "CF1R SecE SclFtr",
                               "CF1RPRF01E_SpecialFeature_PVModuleTypePremium" )
         else  UNCHANGED  endif endif  }
   "Create cf1rSpecialFeature object - PVModuleTypeThinFilm"   ; SAC 5/18/20 (tic #1213)
      cf1rSection_E:Action  = {
         if (Proj:RunScope != 1 .OR. Proj:IsAddAlone > 0 .OR.
             IfValidAnd( Proj:PVWDCSysTotal > 0 )==0 .OR. IfValidAnd( Proj:CommunitySolarProjID > 0 ) .OR. Proj:PVWInputs < 1 .OR.
             (IfValidAnd( Proj:ReducedPVReq > 0 ) .AND. IfValidAnd( Proj:UseReducedPVReqVal = 0 ))) then  UNCHANGED
         else if ( (IfValidAnd( Proj:PVWDCSysSize[1] > 0 ) .AND. Proj:PVWModuleType[1] = 3) .OR.
                   (IfValidAnd( Proj:PVWDCSysSize[2] > 0 ) .AND. Proj:PVWModuleType[2] = 3) .OR.
                   (IfValidAnd( Proj:PVWDCSysSize[3] > 0 ) .AND. Proj:PVWModuleType[3] = 3) .OR.
                   (IfValidAnd( Proj:PVWDCSysSize[4] > 0 ) .AND. Proj:PVWModuleType[4] = 3) .OR.
                   (IfValidAnd( Proj:PVWDCSysSize[5] > 0 ) .AND. Proj:PVWModuleType[5] = 3) )
         then  CreateChildren( cf1rSpecialFeature, 1, "CF1R SecE SclFtr",
                               "CF1RPRF01E_SpecialFeature_PVModuleTypeThinFilm" )
         else  UNCHANGED  endif endif  }
