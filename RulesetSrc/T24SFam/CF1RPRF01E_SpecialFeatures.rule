         if (Proj:RunScope == 1 .AND. Proj:IsAddAlone == 0 .AND.
             IfValidAnd( Proj:ReducedPVReq > 0 ) .AND. Proj:ReducedPVReqExcept > 0 .AND.
             (Proj:AnalysisVersion > 220000 .OR. Proj:ClimateZone = 15 .OR. Proj:ReducedPVReqExcept != 2) .AND.
             IfValidAnd( Proj:CommunitySolarProjID > 0 )==0)
         then  CreateChildren( cf1rSpecialFeature, 1, "CF1R SecE SclFtr",
                               "CF1RPRF01E_SpecialFeature_PVException" )
         else  UNCHANGED  endif  }
