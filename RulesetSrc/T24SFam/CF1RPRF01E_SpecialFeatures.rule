   "Create cf1rSpecialFeature object - PrecoolingCreditTaken"    ; SAC 7/2/19 (tic #1119)
      cf1rSection_E:Action  = {
         if (IfValidAnd( Proj:EnergyCodeYearNum >= 2019 ) .AND.
             IfValidAnd( Proj:UsePreCooling > 0 ) .AND. IfValidAnd( Proj:PreCl_DataValid > 0 ))
         then  CreateChildren( cf1rSpecialFeature, 1, "CF1R SecE SclFtr",
                               "CF1RPRF01E_SpecialFeature_PrecoolingCreditTaken" )
         else  UNCHANGED  endif  }
   "Create cf1rSpecialFeature object - BalancedIAQVentilation"    ; SAC 7/25/19 (tic #1104)   ; SAC 2/19/20 - revised logic to flag ADU IAQ Balanced only when IAQ fan present (tic #1182)
      cf1rSection_E:Action  = {
         if (IfValidAnd( Proj:EnergyCodeYearNum >= 2019 ) .AND.
             ( (Proj:IsMultiFamily < 0.5 .AND.
                   ( IfValidAnd( Proj:UnitIAQCFMBalanced[1] > 0 ) .OR.
                    (IfValidAnd( Proj:UnitIAQCFMBalanced[2] > 0 ) .AND. IfValidAnd( Proj:SFamADUIAQUI > 0 )))) .OR.
               (Proj:IsMultiFamily > 0.5 .AND. SumAll( DwellUnit:IAQCFMIsBalanced ) > 0) ))
         then  CreateChildren( cf1rSpecialFeature, 1, "CF1R SecE SclFtr",
                               "CF1RPRF01E_SpecialFeature_BalancedIAQVentilation" )
         else  UNCHANGED  endif  }
   "Create cf1rSpecialFeature object - IAQVentilationFanEfficacy"    ; SAC 2/10/20 (tic #1174)
      cf1rSection_E:Action  = {
         if (IfValidAnd( Proj:EnergyCodeYearNum >= 2019 ) .AND. SumAll( IAQFan:WperCFMIAQRpt ) > 0 .AND.
             IfValidAnd( Proj:StdDesignIAQWperCFM > 0 ))
         then  if (MinAll( IAQFan:WperCFMIAQRpt ) < Proj:StdDesignIAQWperCFM)
               then  CreateChildren( cf1rSpecialFeature, 1, "CF1R SecE SclFtr",
                                     "CF1RPRF01E_SpecialFeature_IAQVentilationFanEfficacy" )
               else  UNCHANGED  endif
         else  UNCHANGED  endif  }
