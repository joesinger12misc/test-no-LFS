;*************************************************************************
RULELIST "CF1RPRF01E_Final_SpecialFeatures"  1 0 0 1
  ; ----------------------------------------------------------------------
  ; Rules to add/adjust Special Features messages following completion of analysis

   ; PV2kWSystem
     "Reset PV2kWSystem E01b_SpecialFeatureCategoryString"  cf1rSpecialFeature:E01b_SpecialFeatureCategoryString  = {
           if (IfValidAnd( E01_SpecialFeatureCategory = "PV2kWSystem" ) .AND.
               IfValidAnd( Proj:SpeclFeatrs:PVSysCreditVal > 0 ))
           then  Proj:SpeclFeatrs:PVSysCreditMsg
           else  UNCHANGED  endif  }
     "Reset PV2kWSystem E01c_SpecialFeatureCategoryData"  cf1rSpecialFeature:E01c_SpecialFeatureCategoryData  = {
           if (IfValidAnd( E01_SpecialFeatureCategory = "PV2kWSystem" ) .AND.
               IfValidAnd( Proj:SpeclFeatrs:PVSysCreditVal > 0 ))
           then  FltToStr( Proj:SpeclFeatrs:PVSysCreditVal, 2, 0, 0, 1 )
           else  UNCHANGED  endif  }

   ; PVCommunitySolar
     "Reset PVCommunitySolar E01b_SpecialFeatureCategoryString"  cf1rSpecialFeature:E01b_SpecialFeatureCategoryString  = {
           if (IfValidAnd( E01_SpecialFeatureCategory = "PVCommunitySolar" ) .AND.
               IfValidAnd( Proj:SpeclFeatrs:PVSysCreditVal > 0 ))
           then  Format( "Community Solar: %s kWdc of %s", FltToStr( Proj:SpeclFeatrs:PVSysCreditVal, 2, 0, 0, 1 ), EnumString( Proj:CommunitySolarProject ) )
           else  UNCHANGED  endif  }
     "Append additional text onto PVCommunitySolar E01b_SpecialFeatureCategoryString"  cf1rSpecialFeature:E01b_SpecialFeatureCategoryString  =     ; SAC 6/25/20
         {  if (IfValidAnd( E01_SpecialFeatureCategory = "PVCommunitySolar" ) .AND.
                IfValidAnd( Proj:SpeclFeatrs:PVSysCreditVal > 0 ))
            then  switch (Proj:CommunitySolarProject)
                     case  1 :  E01b_SpecialFeatureCategoryString + ". Require SMUD's Attestation of Premise Registration in Neighborhood Solarshares for final inspection"
                     default :  UNCHANGED
                  endswitch
            else  UNCHANGED  endif  }
     "Reset PVCommunitySolar E01c_SpecialFeatureCategoryData"  cf1rSpecialFeature:E01c_SpecialFeatureCategoryData  = {
           if (IfValidAnd( E01_SpecialFeatureCategory = "PVCommunitySolar" ) .AND.
               IfValidAnd( Proj:SpeclFeatrs:PVSysCreditVal > 0 ))
           then  FltToStr( Proj:SpeclFeatrs:PVSysCreditVal, 2, 0, 0, 1 )
           else  UNCHANGED  endif  }
END
