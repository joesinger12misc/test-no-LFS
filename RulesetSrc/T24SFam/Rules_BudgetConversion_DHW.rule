																			else	round( ((ValidOr( InputRatingSim, 0 ) / 800) + (110 * sqrt( TankVolume ))) / (8.25 * TankVolume * 70), 5 )	; frac
																			endif
																	else	UNCHANGED  endif endif
															else	UNCHANGED  endif endif endif endif endif  }

