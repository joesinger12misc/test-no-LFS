         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Climate Zone</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="1">1</dtyp:displayterm>
            <dtyp:displayterm value="10">10</dtyp:displayterm>
            <dtyp:displayterm value="11">11</dtyp:displayterm>
            <dtyp:displayterm value="12">12</dtyp:displayterm>
            <dtyp:displayterm value="13">13</dtyp:displayterm>
            <dtyp:displayterm value="14">14</dtyp:displayterm>
            <dtyp:displayterm value="15">15</dtyp:displayterm>
            <dtyp:displayterm value="16">16</dtyp:displayterm>
            <dtyp:displayterm value="2">2</dtyp:displayterm>
            <dtyp:displayterm value="3">3</dtyp:displayterm>
            <dtyp:displayterm value="4">4</dtyp:displayterm>
            <dtyp:displayterm value="5">5</dtyp:displayterm>
            <dtyp:displayterm value="6">6</dtyp:displayterm>
            <dtyp:displayterm value="7">7</dtyp:displayterm>
            <dtyp:displayterm value="8">8</dtyp:displayterm>
            <dtyp:displayterm value="9">9</dtyp:displayterm>
            <dtyp:displayterm value="AllZones">All climate zones</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="1"/>
         <xsd:enumeration value="10"/>
         <xsd:enumeration value="11"/>
         <xsd:enumeration value="12"/>
         <xsd:enumeration value="13"/>
         <xsd:enumeration value="14"/>
         <xsd:enumeration value="15"/>
         <xsd:enumeration value="16"/>
         <xsd:enumeration value="2"/>
         <xsd:enumeration value="3"/>
         <xsd:enumeration value="4"/>
         <xsd:enumeration value="5"/>
         <xsd:enumeration value="6"/>
         <xsd:enumeration value="7"/>
         <xsd:enumeration value="8"/>
         <xsd:enumeration value="9"/>
         <xsd:enumeration value="AllZones"/>
      </xsd:restriction>
   </xsd:simpleType>
