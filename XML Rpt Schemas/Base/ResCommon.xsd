            <dtyp:displayterm value="Ocean">Ocean</dtyp:displayterm>
            <dtyp:displayterm value="Suburbs">Suburbs</dtyp:displayterm>
            <dtyp:displayterm value="Urban">Urban</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="City"/>
         <xsd:enumeration value="Country"/>
         <xsd:enumeration value="Ocean"/>
         <xsd:enumeration value="Suburbs"/>
         <xsd:enumeration value="Urban"/>
      </xsd:restriction>
   </xsd:simpleType>
