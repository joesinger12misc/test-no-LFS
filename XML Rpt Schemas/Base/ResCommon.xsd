   <xsd:simpleType name="AccessoryDwellingUnitIncluded">
      <xsd:annotation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Accessory Dwelling Unit Attached</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Attached">Attached</dtyp:displayterm>
            <dtyp:displayterm value="Detached">Detached</dtyp:displayterm>
            <dtyp:displayterm value="None">None</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Attached"/>
         <xsd:enumeration value="Detached"/>
         <xsd:enumeration value="None"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="AdditionConditionedFloorArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Addition Conditioned Floor Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="AdditionTotalConditionedFloorArea">
      <xsd:annotation>
         <xsd:documentation>All conditioned floor area in an addition project.  It can be derived from all conditioned area in the Zones  Units: ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Addition Total Conditioned Floor Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="AddressFull">
      <xsd:restriction base="xsd:string">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="AdjacentSpaceName">
      <xsd:annotation>
         <xsd:documentation>This references the space on the other side of an interior partition.</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Adjacent Space Name</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
