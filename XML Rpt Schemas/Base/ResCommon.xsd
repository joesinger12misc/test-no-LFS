            <dtyp:displayterm>Rule Bound Building</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="ProposedDesign">Proposed design</dtyp:displayterm>
            <dtyp:displayterm value="StandardDesign">Standard design</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="ProposedDesign"/>
