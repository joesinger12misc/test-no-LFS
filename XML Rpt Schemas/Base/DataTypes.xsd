		</xsd:annotation>
		<xsd:restriction base="dtyp:Decimal">
			<xsd:maxInclusive value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DecimalNonnegative">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:restriction base="dtyp:Decimal">
			<xsd:minInclusive value="0"/>
		</xsd:restriction>
