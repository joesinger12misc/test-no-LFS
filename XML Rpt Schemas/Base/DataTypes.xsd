		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="0"/>
			<xsd:fractionDigits value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
        <xsd:simpleType name="Decimal5Places">
		<xsd:annotation>
			<xsd:documentation>Basic decimal type with five decimal places.  Units: see derived type.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
